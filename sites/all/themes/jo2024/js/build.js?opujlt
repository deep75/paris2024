var ajaxJoLike = function(){

    var sideHeart = jQuery(".side-heart");
    var velocity = require("velocity-animate");
    var jo_like = jQuery(".hp-header .jo-like");
    //hide menu
    if(jQuery(".hp-header .burger").hasClass("button-active")){
        jQuery(".hp-header .burger").click();
    }
    if(jo_like.hasClass("button-active")){

        jQuery("#page-wrapper").mCustomScrollbar("update");
        killAnimating = true;
        jo_like.removeClass("button-active");
        sideHeart.removeClass('is-open');
        jo_like.removeClass("button-active-loading");
        velocity(sideHeart, {left: "100%"}, {duration: 700,easing: "easeOut"});

        jQuery('.hp-header').removeClass('nav-open');
        jQuery('.hp-header').removeClass('heart-open');
        jQuery('.hp-header').removeClass('hidehomelink');
    }else{
        jQuery("#page-wrapper").mCustomScrollbar("disable");
        killAnimating = false;
        jo_like.addClass("button-active-loading");
        velocity(sideHeart, {left: 0}, {duration: 700,easing: "easeOut"});
        sideHeart.addClass('is-open ajax-load');
        jQuery('.hp-header').addClass('nav-open');
        jQuery('.hp-header').addClass('heart-open');
				jQuery('.hp-header').addClass('hidehomelink');
        if(!jo_like.hasClass('pre-loaded')){
            // get content
            //ajaxLoadJoLike();
            jo_like.addClass("button-active");
            jQuery('#newsletter-form .form-item-email label').insertAfter('#newsletter-form .form-item-email input');

            jQuery('#newsletter-form .form-item-email input').focusout(function() {
                if(jQuery(this).val()){
                    jQuery(this).addClass('active');
                }else{
                    jQuery(this).removeClass('active');
                }
            });
        }else{

            setTimeout(function(){
                jo_like.addClass("button-active");
                killAnimating = true;
            }, 1500);

        }

    }
};

module.exports = function(){


    var heartContainer = jQuery("#heartContainer");


    var heartStroke = document.querySelector('#heartStroke');
    var heartStrokePath = document.querySelector('#heartStroke path');
    var heartGradient = document.querySelector('#heartGradient');
    var heartGradientRect = document.querySelector('#heartGradient rect');
    var heartPathLength = heartStrokePath.getTotalLength();

    var theartPath;
    var theartMask;
    var theartStrokeHide;

    var animating = true;
    var outArea = true;

    var temp = {transform: "matrix(1, 0, 0, 1, 0, 0)"};


    function drawheartIcon(){
        animating = true;

        theartPath = TweenMax.to(heartStrokePath, 1, {strokeDashoffset : 0, delay:0, ease: TweenMax.Quart.easeInOut, onComplete:function(){animating = false;}});

    }

    function overheartIcon(){
        if(!animating && outArea){

            animating = true;

            outArea = false;
            theartPath.kill();
            //sHeartStrokePath.attr({strokeDashoffset: heartPathLength});

            theartStrokeHide = TweenMax.to(heartStroke, 0.5, {opacity : 0, delay:0, ease: TweenMax.Quart.easeInOut, onComplete:function(){
                        heartStroke.style.opacity = 1;
                        heartStrokePath.style.strokeDashoffset = heartPathLength;
                        drawheartIcon();

            }});

            //heartStrokePath.style.strokeDashoffset = heartPathLength;
            //drawheartIcon();
        }
    }


    function leaveheartIcon(){
        outArea = true;
    }


    function onUpdateHandler(){
        heartGradientRect.setAttribute('transform', temp.transform);
    }


    function clickheartIconAnimate(){
        //if(!animating ){

            animating = true;

            heartGradientRect.setAttribute('transform', 'matrix(1, 0, 0, 1, 0, -129)');

            heartGradient.style.opacity = 1;
            heartGradient.style.zIndex = 2;

            heartStrokePath.style.strokeDashoffset = 0;

            theartPath.kill();

            if(theartMask){
                theartMask.kill();
            }


            theartMask = TweenMax.fromTo(temp, 0.5, {transform: "matrix(1, 0, 0, 1, 0, -129)"}, {transform: "matrix(1, 0, 0, 1, 0, 0)", delay:0, ease: TweenMax.Quart.easeIn, onUpdate:onUpdateHandler, onComplete:function(){
                theartMask.kill();
                theartMask = theartMask = TweenMax.fromTo(temp, 0.5, {transform: "matrix(1, 0, 0, 1, 0, 0)"}, {transform: "matrix(1, 0, 0, 1, 0, 129)", delay:0.4, ease: TweenMax.Quart.easeOut, onUpdate:onUpdateHandler, onComplete:function(){
                    animating = false;
                    heartGradient.style.zIndex = 0;
                    heartGradient.style.opacity = 0;
                    if(!killAnimating){
                        clickheartIconAnimate();
                    }


                }});
            }});
        //}
    }

    function clickheartIcon(){
        killAnimating = false;
        ajaxJoLike();
        clickheartIconAnimate();
    }

    function initheartIcon(){
        heartStrokePath.getBoundingClientRect();
        heartGradientRect.getBoundingClientRect();
        heartGradient.getBoundingClientRect();

        heartStrokePath.style.strokeDasharray = heartPathLength + ' ' + heartPathLength;
        heartStrokePath.style.strokeDashoffset = heartPathLength;

        heartGradient.style.opacity = 0;
        heartGradient.style.zIndex = 0;


        heartContainer.on("click", clickheartIcon);

        /* disable use function */
        if(false){
            heartContainer.on("mouseover", overheartIcon);
            heartContainer.on("mouseleave", leaveheartIcon);
        }


        heartContainer.css({visibility:"visible"});

        drawheartIcon();

    }

    initheartIcon();




    // menu animation
    var swiper = require("../modules/swiper");

    var swipers = document.querySelectorAll(".swiper-container");
    if (swipers.length > 0) {
        swiper();
    }

    var nav = document.querySelector(".navbar");
    if (nav) {
        require("../modules/nav")(nav);
    }

    var sideMenu = document.querySelector(".side-menu");

    if (sideMenu) {
        var velocity = require("velocity-animate");


        var toggle = document.querySelector(".hp-header .burger");
        jQuery(".side-menu").css("right", -sideMenu.offsetWidth + "px");

        toggle.addEventListener('click', function () {

            var $el = jQuery(this);
            var $rects = $el.find("svg rect");

            // hide jo like
            var jo_like = jQuery(".hp-header .jo-like");
            if (jo_like.hasClass('button-active')) {
                jo_like.click();
            }

            var defaultWidthRect = 108;
            var disapearWidth = defaultWidthRect + 40;

            if (!jQuery(".hp-header .burger").hasClass("button-active")) {

                jQuery('.hp-header').addClass('nav-open');
                jQuery('.hp-header').removeClass('nav-close');
                jQuery('.side-menu').addClass('menu-open');
                jQuery(".hp-header .burger").addClass("button-active");
                jQuery('.side-menu').removeClass('menu-closed');
                jQuery('.side-menu').addClass('menu-opening');
                setTimeout(function () {
                    jQuery('.side-menu').addClass('menu-opened');
                    jQuery('.side-menu').removeClass('menu-opening');
                    jQuery('.menu-bg-black').show();
                    velocity(jQuery('.menu-bg-black').eq(0), {opacity: 0.6}, {delay: 400, duration: 100});
                }, 1);
                setTimeout(function () {
                    $el.css('visibility', 'visible');
                    var mySequence = [
                        {e: $rects.eq(0), p: {translateX: disapearWidth}, o: {duration: 250}},
                        {
                            e: $rects.eq(1),
                            p: {translateX: -disapearWidth},
                            o: {duration: 250, sequenceQueue: false}
                        },
                        {e: $rects.eq(2), p: {translateX: disapearWidth}, o: {duration: 250, sequenceQueue: false}},
                        {
                            e: $rects.eq(0),
                            p: {translateY: 5, translateX: 20, rotateZ: 45, width: 0},
                            o: {duration: 0, delay: 0, sequenceQueue: false}
                        },
                        {
                            e: $rects.eq(2),
                            p: {translateY: 73, translateX: 153, rotateZ: 135, width: 0},
                            o: {duration: 0, delay: 0, sequenceQueue: false}
                        },
                        {e: $rects.eq(0), p: {width: defaultWidthRect}, o: {duration: 250}},
                        {
                            e: $rects.eq(2),
                            p: {width: defaultWidthRect},
                            o: {duration: 250, delay: 120, sequenceQueue: false}
                        }
                    ];
                    velocity.RunSequence(mySequence);
                }, 10);
                velocity(sideMenu, {right: 0}, {
                        easing: "easeOut",
                        complete: function () {

                        }
                    }
                );
                jQuery('.side-menu .menu-wrapper').mCustomScrollbar({
                    theme: "dark",
                    scrollbarPosition: "outside",
                    axis: "y",
                    keyboard: {scrollAmount: 40},
                    mouseWheel: {deltaFactor: 40},
                    scrollInertia: 700
                });
                if(jQuery(document).width() < 768){
                    jQuery('#page-wrapper').mCustomScrollbar("disable");
                }
