var div_vid = jQuery(".element-active .event-video");

//var btnPlayContainer = jQuery(".element-active .get-video");
var TweenMax = require("./../../../bower_components/gsap/src/minified/TweenMax.min");

var btnPlayGrey = document.querySelector('#btnPlayStrokeGrey');
var btnPlayGreytRect = document.querySelector('#btnPlayStrokeGrey rect');
var btnPlayGradientRect = document.querySelector('#btnPlayGradient rect');

var tBtnPlayMaskGrey;
var tBtnPlayMask;

var animatingBtnPlay = false;
var loadingBtnPlay = false;

var btnPlayID;

function btnPlayAnimKill(){
	animatingBtnPlay = false;
	loadingBtnPlay = false;
	btnPlayGrey.style.opacity = 1;
	if(tBtnPlayMaskGrey){
		tBtnPlayMaskGrey.kill();
	}
	if(tBtnPlayMask){
		tBtnPlayMask.kill();
	}
	btnPlayGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');
	btnPlayGradientRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 0, 0)');
	clearTimeout(btnPlayID);
}



var temp8 = {transform: "matrix(0, 0, 0, 1, 0, 0)"};

function onUpdateHandler8() {
	btnPlayGradientRect.setAttribute('transform', temp8.transform);
}

function btnPlayLoadAnim() {

	if (tBtnPlayMask) {
		tBtnPlayMask.kill();
	}

	animatingBtnPlay = true;


	tBtnPlayMask = TweenMax.fromTo(temp8, 0.6, {transform: "matrix(0, 0, 0, 1, 0, 0)"}, {
		transform: "matrix(1, 0, 0, 1, 0, 0)",
		delay: 0,
		ease: TweenMax.Quart.easeIn,
		onUpdate: onUpdateHandler8,
		onComplete: function () {
			tBtnPlayMask.kill();
			tBtnPlayMask = TweenMax.fromTo(temp8, 0.5, {transform: "matrix(1, 0, 0, 1, 0, 0)"}, {
				transform: "matrix(0, 0, 0, 1, 65, 0)",
				delay: 0.2,
				ease: TweenMax.Quart.easeOut,
				onUpdate: onUpdateHandler8,
				onComplete: function () {
					btnPlayID = setTimeout(btnPlayLoadAnim, 50);
				}
			});
		}
	});

}

var temp7 = {transform: "matrix(0, 0, 0, 1, 20, 0)"};

function onUpdateHandler7() {
	btnPlayGreytRect.setAttribute('transform', temp7.transform);
}
function clickBtnPlayIcon() {
	if (!animatingBtnPlay && !loadingBtnPlay) {
		animatingBtnPlay = true;
		if (tBtnPlayMaskGrey) {
			tBtnPlayMaskGrey.kill();
		}

		btnPlayGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');


		tBtnPlayMaskGrey = TweenMax.fromTo(temp7, 0.3, {transform: "matrix(0, 0, 0, 1, 20, 0)"}, {
			transform: "matrix(1, 0, 0, 1, 0, 0)",
			delay: 0,
			ease: TweenMax.Quart.easeInOut,
			onUpdate: onUpdateHandler7,
			onComplete: function () {
				tBtnPlayMaskGrey.kill();
				loadingBtnPlay = true;
				tBtnPlayMaskGrey = TweenMax.fromTo(btnPlayGrey, 1, {opacity: 1}, {
					opacity: 0.4,
					delay: 0,
					ease: TweenMax.Quart.easeInOut,
					onComplete: btnPlayLoadAnim
				});
			}
		});
	}
}


function initBtnPlayIcon() {

	btnPlayGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');
	btnPlayGradientRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 0, 0)');

}

if (jQuery("#btnPlayContainer").length) {
	initBtnPlayIcon();
}


function withVelocity() {
	jQuery(".element-active .video").removeClass('hide');
	div_vid = jQuery(".element-active .event-video");
	div_vid.addClass('video-hidden');
	var margin = window.innerWidth * 2.2 / 2;
	var video = jQuery(".element-active .video video").get(0);
	if(jQuery(".element-active .video video").length){
		//video.load();
		video.play();
	}

	div_vid.velocity({
		width: window.innerWidth * 2.2,
		height: window.innerWidth * 2.2,
		'margin-right': 0 - margin,
		'margin-bottom': 0 - margin
	}, {
		duration: 550,
		easing: "easeInQuad",
		complete: function () {
			jQuery(".get-video__close").removeClass('hide');
		}
	});
}
function hideVelocity() {
	div_vid = jQuery(".element-active .event-video");
	div_vid.velocity({
		width: 0,
		height: 0,
		'margin-right': 0,
		'margin-bottom': 0
	}, {
		duration: 850,
		easing: "easeOutQuad",
		complete: function () {
			jQuery(".element-active .video").addClass('hide');
			div_vid.removeClass('video-hidden');
			var video = jQuery(".element-active .video video").get(0);
			if(jQuery(".element-active .video video").length){
				video.stop();
			}


		}
	});

}

var containerVideoWrap = jQuery(".get-video-wrap");
if (jQuery(window).get(0).innerWidth > jQuery(window).get(0).innerHeight) {
	var heightwrap = jQuery(window).get(0).innerHeight * 0.8;
	if(jQuery(window).get(0).innerHeight<=700){
		var ratioK=jQuery(window).get(0).innerHeight/jQuery(window).get(0).innerWidth;
		ratioK=ratioK + 0.15;
		heightwrap=jQuery(window).get(0).innerHeight * ratioK;
	}
	containerVideoWrap.width(heightwrap);
	containerVideoWrap.height(heightwrap);
	var correctWrapTop = (jQuery(window).get(0).innerHeight - heightwrap) / 2;
	var correctWrapLeft = (jQuery(window).get(0).innerWidth - heightwrap) / 2;
	containerVideoWrap.css('margin-top', correctWrapTop + 'px');
	containerVideoWrap.css('margin-left', correctWrapLeft + 'px');
}else{
	var widthCount = jQuery(window).get(0).innerWidth * 0.4;
	containerVideoWrap.width(widthCount);
	containerVideoWrap.height(widthCount);
	var correctTop2 = (jQuery(window).get(0).innerHeight - widthCount) / 2;
	var correctLeft2 = (jQuery(window).get(0).innerWidth - widthCount) / 2;
	containerVideoWrap.css('margin-top', correctTop2 + 'px');
	containerVideoWrap.css('margin-left', correctLeft2 + 'px');
}
jQuery(window).resize(function () {
	if (jQuery(window).get(0).innerWidth > jQuery(window).get(0).innerHeight) {
		var heightwrap = jQuery(window).get(0).innerHeight * 0.8;
		if(jQuery(window).get(0).innerHeight<=700){
			var ratioK=jQuery(window).get(0).innerHeight/jQuery(window).get(0).innerWidth;
			ratioK=ratioK + 0.15;
			heightwrap=jQuery(window).get(0).innerHeight * ratioK;
		}
	containerVideoWrap.width(heightwrap);
	containerVideoWrap.height(heightwrap);
	correctWrapTop = (jQuery(window).get(0).innerHeight - heightwrap) / 2;
	correctWrapLeft = (jQuery(window).get(0).innerWidth - heightwrap) / 2;
	containerVideoWrap.css('margin-top', correctWrapTop + 'px');
	containerVideoWrap.css('margin-left', correctWrapLeft + 'px');
	}else{
		var widthCount = jQuery(window).get(0).innerWidth * 0.4;
		containerVideoWrap.width(widthCount);
		containerVideoWrap.height(widthCount);
		var correctTop2 = (jQuery(window).get(0).innerHeight - widthCount) / 2;
		var correctLeft2 = (jQuery(window).get(0).innerWidth - widthCount) / 2;
		containerVideoWrap.css('margin-top', correctTop2 + 'px');
		containerVideoWrap.css('margin-left', correctLeft2 + 'px');
	}
