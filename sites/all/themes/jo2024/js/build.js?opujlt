jQuery(document).on("mouseenter", ".hp-content .get-video-wrap", function () {
	jQuery(this).addClass('hover-in');
	jQuery(this).removeClass('hover-out');
});
jQuery(document).on("mouseleave", ".hp-content .get-video-wrap", function () {
	jQuery(this).removeClass('hover-in');
