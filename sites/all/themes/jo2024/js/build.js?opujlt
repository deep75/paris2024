    // `go()` is executed when the DOM is ready,
    // if anything in your code needs to touch the DOM,
    // put it into this function (or any other function
    // called in this function).

    var go = function () {
        require("velocity-animate/velocity.ui");



        require("./modules/bidbook")();
        require("./modules/animation")();
        require("./modules/video-swiper");
        require("./modules/mobile-js");

        rotator();
    };

    jQuery(document).ready(scripts);

    ready(go);
})();

