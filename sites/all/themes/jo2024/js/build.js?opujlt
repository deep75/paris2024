// 	page detail hp layer interne
function apply_scroll_push() {

	if (jQuery('.block-fade-in-up').length > 0) {
		jQuery('.block-fade-in-up').addClass("show");

	}
	if (jQuery('.block-fade-in').length > 0) {
		jQuery('.block-fade-in').each(function () {
			var bottom_of_object = jQuery(this).offset().top + jQuery(this).outerHeight();
			var bottom_of_window = jQuery(window).scrollTop() + jQuery(window).height();

			if (bottom_of_window >= bottom_of_object) {
				jQuery(this).addClass("show");
			}

		});
	}
}
var load = false;
function loadTimelineAjax() {
	// timeline more elements

	var limit_cible = jQuery('#loadmoreajaxloader');
	var limit = limit_cible.attr("limit");
	var last_year_displayed = jQuery('.timeline-title:last').attr("data-id");
	var count_data = jQuery('#postswrapper');
	//252 == height of footer
	var bottom_of_object = jQuery(".timeline.page-timeline").offset().top + jQuery(".timeline.page-timeline").outerHeight();
	var bottom_of_window = jQuery(window).scrollTop() + jQuery(window).height();
	if (bottom_of_window >= bottom_of_object) {
		var nbrCountData = count_data.attr("data-count");
		nbrCountData = parseInt(nbrCountData, 10);
		if (nbrCountData > 10) {
			if (load === false) {
				load = true;
				jQuery('div#loadmoreajaxloader').show();
				jQuery.ajax({
					type: 'POST',
					url: jQuery('#page-wrapper').attr('data-base-url') + '/jo2024/ajax/timeline',
					data: {
						'limit': limit,
						'last_year_displayed': last_year_displayed
					},
					success: function (datarespo) {

						var responseData = jQuery.parseJSON(datarespo);

						var dataHtml;
						jQuery.each(responseData, function (i, val) {
							if (val.command === 'insert') {
								dataHtml = val.data;
							}
						});
						if (dataHtml) {

							limit_cible.attr("limit", parseInt(limit, 10) + 10);
							jQuery("#postswrapper").append(dataHtml);
						}
					}
				}).complete(function () {
					count_data.attr("data-count", count_data.attr("data-count") - 10);
					load = false;
					jQuery('div#loadmoreajaxloader').hide();
					var width = jQuery('.embedded-video iframe').width();
					width = width * 0.5625;
					jQuery('.embedded-video iframe').css('height', width + "px");
				});
			}
		}
	}
}
var load_press = false;
function loadPressAjax() {


	//var offset          = jQuery('.timeline').offset();
	var limit_cible = jQuery('#loadmoreajaxloader');
	var limit = limit_cible.attr("limit");

	var count_data = jQuery('#postswrapper');
	var bottom_of_object = jQuery(".page-press .block-press").offset().top + jQuery(".page-press .block-press").outerHeight();
	var bottom_of_window = jQuery(window).scrollTop() + jQuery(window).height();

	if (bottom_of_window >= bottom_of_object) {
		if (parseInt(count_data.attr("data-count"), 10) > 10) {
			if (load_press === false) {
				load_press = true;

				jQuery('div#loadmoreajaxloader').show();
				jQuery.ajax({
					type: 'POST',
					url: jQuery('#page-wrapper').attr('data-base-url') + '/jo2024/ajax/press',
					data: {
						'limit': limit
					},
					success: function (html) {
						if (html) {
							limit_cible.attr("limit", parseInt(limit, 10) + 10);
							jQuery("#postswrapper").append(html);
						}
					}
				}).complete(function () {
					count_data.attr("data-count", count_data.attr("data-count") - 10);
					load_press = false;
					jQuery('div#loadmoreajaxloader').hide();
				});
			}
		}
	}

}

var directionScroll = 0, isscrolling = false;

function scrolleBloc_n1(directionScroll, scrollRatio) {

	if (jQuery('#jo-wrapper-detail').length) {
		if (jQuery('#jo-wrapper-detail').offset().top > 0 && directionScroll === 1) {

			jQuery("#page-wrapper").mCustomScrollbar("stop");
			jQuery("#page-wrapper").mCustomScrollbar("disableMouseWheel");
			customScrollToBloc = false;

			isscrolling = false;
			jQuery(".to-top").addClass("hide");
			jQuery(".hp-header").removeClass("nav-intern");
			jQuery("body").removeClass("block-n-1");


			velocity(jQuery('#mCSB_1_container'), {top: 0}, {
				duration: 600, easing: "easeOut", complete: function () {
					customScrollToBloc = true;
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", "top", {
						scrollInertia: 0,
						callbacks: false
					});
					jQuery("#page-wrapper").mCustomScrollbar("enableMouseWheel");
				}
			});
			velocity(jQuery('#mCSB_1_dragger_vertical'), {top: 0}, {
				duration: 600, easing: "easeOut", complete: function () {
				}
			});
		} else if (jQuery('#jo-wrapper-detail').offset().top > 0 && directionScroll === -1) {


			isscrolling = false;
			jQuery("#page-wrapper").mCustomScrollbar("stop");
			jQuery("#page-wrapper").mCustomScrollbar("disableMouseWheel");
			customScrollToBloc = false;
			if (jQuery(jQuery(".to-top").hasClass('hide'))) {
				jQuery(".to-top").removeClass("hide");
			}

			jQuery(".hp-header").addClass("nav-intern");
			jQuery("body").addClass("block-n-1");

			//jQuery("#page-wrapper").mCustomScrollbar("scrollTo",jQuery('#jo-wrapper-detail'), false);
			velocity(jQuery('#mCSB_1_container'), {top: -jQuery('.hp-content .swiper-container').height()}, {
				duration: 600, easing: "easeOut", complete: function () {
					customScrollToBloc = true;
					jQuery("#page-wrapper").mCustomScrollbar("enableMouseWheel");
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery('#jo-wrapper-detail'), {
						scrollInertia: 0,
						callbacks: false
					});
					//resizeCustumScroll();
				}
			});
			var swiperHeight = jQuery('.hp-content .swiper-container').height();
			swiperHeight = parseInt(swiperHeight, 10);
			var todraggable = swiperHeight / scrollRatio;
			todraggable = parseInt(todraggable, 10);
			velocity(jQuery('#mCSB_1_dragger_vertical'), {top: todraggable}, {
				duration: 600, easing: "easeOut", complete: function () {
					customScrollToBloc = true;
					jQuery("#page-wrapper").mCustomScrollbar("enableMouseWheel");
					//resizeCustumScroll();
				}
			});
		}

	}
}
function pagePartenaireScrollBlock(top, scrollRatio){

	jQuery("#page-wrapper").mCustomScrollbar("stop");
	jQuery("#page-wrapper").mCustomScrollbar("disableMouseWheel");
	customScrollToBloc = false;

	isscrolling = false;

	velocity(jQuery('#mCSB_1_container'), {top: -top}, {
		duration: 600, easing: "easeOut", complete: function () {
			customScrollToBloc = true;
			jQuery("#page-wrapper").mCustomScrollbar("enableMouseWheel");
			jQuery("#page-wrapper").mCustomScrollbar("scrollTo",top,{
				scrollInertia:0,
				callbacks:false
			});
			//resizeCustumScroll();
		}
	});

	var swiperHeight = top;
	swiperHeight = parseInt(swiperHeight, 10);
	var todraggable = swiperHeight / scrollRatio;
	todraggable = parseInt(todraggable, 10);
	velocity(jQuery('#mCSB_1_dragger_vertical'), {top: todraggable}, {
		duration: 600, easing: "easeOut", complete: function () {
			customScrollToBloc = true;
			jQuery("#page-wrapper").mCustomScrollbar("enableMouseWheel");
			//resizeCustumScroll();
		}
	});

}

function pagePartenaireScroll(directionScroll, scrollRatio){


	var partMobilise = jQuery('.partenaire-mobilse');
	var partHeader = jQuery('.header-partenaire');
	//var partEngage = jQuery('.partenaire-engage');
	var partBothHeight = partMobilise.outerHeight() + partHeader.outerHeight();


	// scroll to bottom
	if(directionScroll === -1){

		if (jQuery('.partenaire-mobilse').offset().top > 0) {
			// bloc 1
			jQuery('.to-top').removeClass('hide');

			pagePartenaireScrollBlock(partHeader.outerHeight(), scrollRatio);

				setTimeout(function(){
					moveButtonPartenaire();
				}, 700);

		} else if (jQuery('.partenaire-mobilse').offset().top >= -partMobilise.outerHeight()) {
			jQuery('.to-top').removeClass('hide');
			pagePartenaireScrollBlock(partBothHeight, scrollRatio);
		}
	}else{
		if (jQuery('.partenaire-mobilse').offset().top > 0) {
			jQuery('.to-top').addClass('hide');
			setTimeout(function(){
				moveButtonPartenaire();
			}, 700);
			pagePartenaireScrollBlock(0, scrollRatio);
		} else if (jQuery('.partenaire-mobilse').offset().top >= -partMobilise.outerHeight()) {
			jQuery('.to-top').removeClass('hide');
			pagePartenaireScrollBlock(partHeader.innerHeight(), scrollRatio);
		}
	}

}
var scrollKey = false;
var scrollDir = 0;



jQuery("#page-wrapper").mCustomScrollbar({
	theme: "dark",
	scrollbarPosition: "outside",
	axis: "y",
	keyboard: {scrollAmount: 40},
	mouseWheel: {deltaFactor: 40},
	scrollInertia: 700,
	callbacks: {
		onScrollStart: function () {
			isscrolling = true;

		},
		onScroll: function () {
			isscrolling = false;
			customScrollToBloc = true;
		},
		whileScrolling: function () {
			directionScroll = this.mcs.scrollDirection;
			scrollDir = this.mcs.scrollKeyboard;

			if (jQuery('#jo-wrapper-detail').length) {
				if (parseInt(scrollDir, 10) !== scrollDir) {
					scrollKey = true;
					if (jQuery('#jo-wrapper-detail').length && jQuery('#jo-wrapper-detail').offset().top > 0 && jQuery(".hp-header").hasClass("nav-intern")) {
						jQuery(".hp-header").removeClass("nav-intern");
						jQuery(".to-top").addClass("hide");
					} else if (jQuery('#jo-wrapper-detail').length && jQuery('#jo-wrapper-detail').offset().top < 0 && !jQuery(".hp-header").hasClass("nav-intern")) {
						jQuery(".hp-header").addClass("nav-intern");
						jQuery(".to-top").removeClass("hide");
					}

				} else {
					scrollKey = false;
				}
				if (customScrollToBloc && !scrollKey && !startSwiper1) {

					scrolleBloc_n1(directionScroll, this.mcs.scrolltoVal);
				}
				apply_scroll_push();
			}

			// load ajax timeline
			if (jQuery('.timeline.page-timeline').length > 0) {
				loadTimelineAjax();
			}

			// load ajax page press
			if (jQuery('.page-press .block-press').length > 0) {
				loadPressAjax();
			}

			// display button to-top internal page

			if(jQuery('header.not-front-page').length && !jQuery('#page-partenaires').length){
				var siteContentTop = jQuery('.site-content').offset().top;
				if (parseInt(siteContentTop, 10) < -10) {
					jQuery('.to-top').removeClass('hide');
				} else {
					jQuery('.to-top').addClass('hide');
				}
			}
			if(jQuery('#page-partenaires').length){
				// && !startSwiper2

				if (customScrollToBloc && !disableScrollPartMobile && !startSwiper2) {
					pagePartenaireScroll(directionScroll, this.mcs.scrolltoVal);
				}
			}
		}
	}
});


//jQuery("#page-wrapper").resizable();


// adjustImageSize()
// -----------------
// Resize cover images proportionally to the window's aspect ratio
function adjustImageSize() {
	// list of cover images
	var imgs = document.querySelectorAll(".swiper-slide .cover");
	var swipercontainer = document.querySelector(".swiper-container");
	var swiperwrapper = document.querySelector(".swiper-wrapper");
	var swiperslide = document.querySelectorAll(".swiper-slide");
	var imagewrapper = document.querySelectorAll(".image-wrapper");

	// window aspect ratio


	var windowWidthA = jQuery(window).innerWidth();
	var windowHeightA = jQuery(window).innerHeight();

	var R = windowWidthA / windowHeightA;
	for (var i = 0, len = swiperslide.length; i < len; i++) {
		swiperslide[i].style.height = "100vh";
		swiperslide[i].style.width = "100vw";

	}
	for (i = 0, len = imagewrapper.length; i < len; i++) {
		imagewrapper[i].style.height = "100vh";
		imagewrapper[i].style.width = "100vw";
	}
	for (i = 0, len = imgs.length; i < len; i++) {
		var is_img = hasClass(imgs[i], "is-image");
		var r;
		if (is_img) {
			r = imgs[i].naturalWidth / imgs[i].naturalHeight;
		} else {
			r = imgs[i].videoWidth / imgs[i].videoHeight;
		}
		var imageWidthA;
		var imageHeightA;


		if (is_img) {
			imageWidthA = imgs[i].naturalWidth;
			imageHeightA = imgs[i].naturalHeight;
		} else {
			imageWidthA = imgs[i].videoWidth;
			imageHeightA = imgs[i].videoHeight;
		}


		swipercontainer.style.height = "100vh";
		swipercontainer.style.width = "100vw";
		swiperwrapper.style.height = "100vh";
		swiperwrapper.style.width = "100vw";


		var caseImage;
		/*if(windowHeightA >= windowWidthA) {
		 if(imageWidthA > imageHeightA) {
		 caseImage = 1;
		 } else {
		 caseImage = 2;
		 }
		 } else {
		 if(imageWidthA >= imageHeightA) {
		 caseImage = 2;
		 } else {
		 caseImage = 1;
		 }
		 }*/

		if (R === r) {
			caseImage = 3;
		} else if (R > r) {
			caseImage = 2;
		} else if (R < r) {
			caseImage = 1;
		}

		switch (caseImage) {
			case 1:
				imgs[i].style.height = "100vh";
				if (windowWidthA < 768) {
					var addheight = windowHeightA + 10;
					imgs[i].style.height = addheight + "px";
				}
				imgs[i].style.left = "50%";
				imgs[i].style.marginLeft = 0 - (windowWidthA / 2 + (imgs[i].offsetWidth - windowWidthA) / 2) + "px";
				imgs[i].style.marginTop = 0;
				imgs[i].style.top = 0;
				imgs[i].style.bottom = 0;
				if (windowWidthA < 768) {
					imgs[i].style.top = 'auto';
					imgs[i].style.bottom = '-5px';
				}
				imgs[i].style.width = "auto";
				break;
			case 2:
				imgs[i].style.height = "auto";
				imgs[i].style.left = 0;
				imgs[i].style.marginLeft = 0;
				imgs[i].style.marginTop = 0 - (windowHeightA / 2 + (imgs[i].offsetHeight - windowHeightA) / 2) + "px";
				imgs[i].style.top = "50%";
				imgs[i].style.width = "100vw";
				break;
			default:
				imgs[i].style.height = "auto";
				imgs[i].style.left = 0;
				imgs[i].style.marginLeft = 0;
				imgs[i].style.marginTop = 0;
				imgs[i].style.top = 0;
				imgs[i].style.width = "auto";
		}
	}
}

/*
 // adjust one image or video
 function adjustOneImageSize(img) {
 // list of cover images

 // window aspect ratio
 var R = window.innerWidth / window.innerHeight;
 var is_img = hasClass(img, "is-image");
 var r;
 if (is_img) {
 r = img.naturalWidth / img.naturalHeight;
 } else {
 r = img.videoWidth / img.videoHeight;
 }


 if (R < r) {
 // cover image ratio is higher than window ratio
 img.style.height = "100vh";
 img.style.left = "50%";
 img.style.marginLeft = 0 - (window.innerWidth / 2 + (img.offsetWidth - window.innerWidth) / 2) + "px";
 img.style.marginTop = 0;
 img.style.top = 0;
 img.style.width = "auto";
 } else if (R > r) {
 // cover image ratio is lower than window ratio
 img.style.height = "auto";
 img.style.left = 0;
 img.style.marginLeft = 0;
 img.style.marginTop = 0 - (window.innerHeight / 2 + (img.offsetHeight - window.innerHeight) / 2) + "px";
 img.style.top = "50%";
 img.style.width = "100vw";
 } else {
 // both ratios are equal
 img.style.height = "auto";
 img.style.left = 0;
 img.style.marginLeft = 0;
 img.style.marginTop = 0;
 img.style.top = 0;
 img.style.width = "auto";
 }
 }*/


if (jQuery('#container').length > 0) {
	var TweenMax = require("./../../../bower_components/gsap/src/minified/TweenMax.min");

	var container = jQuery("#container");

	container.width(jQuery(window).get(0).innerWidth);
	container.height(jQuery(window).get(0).innerHeight);

	var layer1 = jQuery("#layer1");
	var ctx1 = layer1.get(0).getContext('2d');
	layer1.attr("width", container.width());
	layer1.attr("height", container.height());

	var layer2 = jQuery("#layer2");
	var ctx2 = layer2.get(0).getContext('2d');
	layer2.attr("width", container.width());
	layer2.attr("height", container.height());

	var layer3 = jQuery("#layer3");
	var ctx3 = layer3.get(0).getContext('2d');
	layer3.attr("width", container.width());
	layer3.attr("height", container.height());

	var centerX = container.width() / 2;
	var centerY = container.height() / 2;

	var drawLineWidth = container.width() * 0.50;

	var gradient;


	var repeatID;

	var tline;
	var t1line2;
	var t2line2;

	var line = {
		width: drawLineWidth,
		x1: centerX,
		x2: centerX,
		y: centerY,

		draw: function () {
			var middleWidth = this.width / 2;
			this.x1 = centerX - middleWidth;
			this.x2 = centerX + middleWidth;
			ctx2.clearRect(0, 0, container.width(), container.height());
			ctx2.beginPath();
			ctx2.moveTo(this.x1, this.y);
			ctx2.lineTo(this.x2, this.y);
			ctx2.closePath();
			ctx2.stroke();
		}
	};

	var line2 = {

		x1: centerX - drawLineWidth / 2,
		x2: centerX + drawLineWidth / 2,

		y: centerY,

		draw: function () {

			gradient = ctx3.createLinearGradient(this.x1, this.y, this.x2, this.y);
			gradient.addColorStop(0, "rgb(0, 183, 225)"); //blue
			gradient.addColorStop(1, "rgb(30, 52, 129)"); // green

			ctx3.strokeStyle = gradient;

			ctx3.clearRect(0, 0, container.width(), container.height());
			ctx3.beginPath();
			ctx3.moveTo(this.x1, this.y);
			ctx3.lineTo(this.x2, this.y);
			ctx3.closePath();
			ctx3.stroke();
		}
	};
}
function drawLine() {
	line.draw();
}

function initLine2() {

	line2.x1 = centerX - drawLineWidth / 2;
	line2.x2 = centerX + drawLineWidth / 2;
	ctx3.globalAlpha = 1;
	t1line2 = TweenMax.to(line2, 1, {x1: centerX + drawLineWidth / 2, delay: 1, ease: TweenMax.Power2.easeInOut});
	t1line2 = TweenMax.from(line2, 2, {
		x2: centerX - drawLineWidth / 2, delay: 0, ease: TweenMax.Power2.easeInOut,
		onComplete: function () {
			ctx3.globalAlpha = 0;
			clearTimeout(repeatID);
			if (t1line2) {
				t1line2.kill();
			}
			if (t2line2) {
				t2line2.kill();
			}
			repeatID = setTimeout(initLine2, 200);
		}
	});
}

function drawLine2() {
	line2.draw();
}

function stopDrawLine() {

	if (tline) {
		tline.kill();
	}

	initLine2();

	TweenMax.ticker.removeEventListener("tick", drawLine);
	TweenMax.ticker.addEventListener("tick", drawLine2);
}

function settingCanvas() {
	//ctx2.shadowBlur = 6;
	//ctx2.shadowColor = "rgba(100, 100, 100,.6)";
	ctx2.lineCap = "round";
	ctx2.strokeStyle = "rgba(255,255,255,1)";
	ctx2.lineWidth = 4;

	ctx3.lineCap = "round";
	ctx3.strokeStyle = gradient;
	ctx3.lineWidth = 4;

	var gradientBg = ctx1.createLinearGradient(0, 0, container.width(), container.height());
	gradientBg.addColorStop(0, "rgb(138, 211, 238)"); //blue
	gradientBg.addColorStop(0.5, "rgb(0, 165, 217)"); //blue
	gradientBg.addColorStop(1, "rgb(25, 36, 83)"); // green

	ctx1.fillStyle = gradientBg;

}

function init() {
	settingCanvas();

	ctx1.clearRect(0, 0, container.width(), container.height());
	ctx1.fillRect(0, 0, container.width(), container.height());

	tline = TweenMax.from(line, 0.6, {width: 0, delay: 0, ease: TweenMax.Quart.easeInOut, onComplete: stopDrawLine});

	TweenMax.ticker.addEventListener("tick", drawLine);
}

function killPreloader() {

	TweenMax.ticker.removeEventListener("tick", drawLine2);

	TweenMax.ticker.removeEventListener("tick", drawLine);
	if (tline) {
		tline.kill();
	}

	if (t1line2) {
		t1line2.kill();
	}

	if (t2line2) {
		t2line2.kill();
	}
	clearTimeout(repeatID);
}

function resizeCanvas() {
	container.width(jQuery(window).get(0).innerWidth);
	container.height(jQuery(window).get(0).innerHeight);

	layer1.attr("width", container.width());
	layer1.attr("height", container.height());

	layer2.attr("width", container.width());
	layer2.attr("height", container.height());
	layer3.attr("width", container.width());
	layer3.attr("height", container.height());

	centerX = container.width() / 2;
	centerY = container.height() / 2;

	drawLineWidth = container.width() * 0.60;

	line.width = drawLineWidth;
	line.x1 = centerX;
	line.x2 = centerX;
	line.y = centerY;

	line2.x1 = centerX - drawLineWidth / 2;
	line2.x2 = centerX + drawLineWidth / 2;
	line2.y = centerY;

	TweenMax.ticker.removeEventListener("tick", drawLine2);

	TweenMax.ticker.removeEventListener("tick", drawLine);

	if (tline) {
		tline.kill();
	}
	if (t1line2) {
		t1line2.kill();
	}

	init();
}


if (jQuery('#container-counter').length > 0) {
	var container0 = jQuery("#container-counter");
	var containerGlobal = jQuery(".intro-counter");

	var layer00 = jQuery("#layer00");
	var ctx00 = layer00.get(0).getContext('2d');

	var layer01 = jQuery("#layer01");
	var ctx01 = layer01.get(0).getContext('2d');

	var layer02 = jQuery("#layer02");
	var ctx02 = layer02.get(0).getContext('2d');

	var layer03 = jQuery("#layer03");
	var ctx03 = layer03.get(0).getContext('2d');

	var layer04 = jQuery("#layer04");
	var ctx04 = layer04.get(0).getContext('2d');

	var layer05 = jQuery("#layer05");
	var ctx05 = layer05.get(0).getContext('2d');

	var layer06 = jQuery("#layer06");
	var ctx06 = layer06.get(0).getContext('2d');

	var rgba;
	var rgba2;
	var rgba3;

	var endVal = 0;
	var currVal = 0;


	var centerX0 = container0.width() / 2;
	var centerY0 = container0.height() / 2;

	var per = 0;

	var numDays = 0;

	var reverseID;

	var lineWidth = 16;

	var fontSize = 74;

	var drawLineWidth0 = container0.width() * 0.50;

	var currentDate;
	var endDate = new Date('2017-09-13');
	var elapsed;

	var timerGrayCercle;
	var timerGrayCercleReverse;
	var timerDrawingTexts;

	var timerText01;
	var timerText02;
	var timerText03;

	var timerFadeCercle;
	var timerLineTwoCercle;

	var gradient0 = ctx01.createLinearGradient(0, 0, container0.width(), container0.height());
	gradient0.addColorStop(0, "rgb(0, 183, 225)"); //blue
	gradient0.addColorStop(1, "rgb(30, 52, 129)"); // blue 2

	var obj = {
		getTextVal: function () {
			return parseInt(currVal, 10);
		},

		setTextVal: function (val) {
			currVal = parseInt(val, 10);
			numDays = currVal;
		}
	};


	var text1 = {
		text: 'Rdv à Lima',
		x: centerX0,
		y: centerY0 * 2 * 0.43,
		alpha: 1,

		draw: function () {
			rgba = 'rgba(255,255,255,' + this.alpha + ')';

			ctx02.clearRect(0, 0, container0.width(), container0.height());
			ctx02.fillStyle = 'rgba(255,0,0,1)';
			ctx02.fillRect(0, centerY0 * 2 * 0.6, centerX0 * 2, centerX0 * 2);
			ctx02.fillStyle = rgba;
			ctx02.fillText(this.text, this.x, this.y);
		}
	};

	var text2 = {
		text: '13.09.2017',
		x: centerX0,
		y: centerY0 * 2 * 0.55,
		alpha: 1,

		draw: function () {
			rgba2 = 'rgba(255,255,255,' + this.alpha + ')';
			ctx03.clearRect(0, 0, container0.width(), container0.height());
			ctx03.fillStyle = 'rgba(255,0,0,1)';
			ctx03.fillRect(0, centerY0 * 2 * 0.6, centerX0 * 2, centerX0 * 2);
			ctx03.fillStyle = rgba2;
			ctx03.fillText(this.text, this.x, this.y);
		}
	};

	var text3 = {
		text: 'j - 0',
		x: centerX0,
		y: centerY0 * 2 * 0.66,
		alpha: 1,

		draw: function () {
			this.text = 'j - ' + numDays;
			rgba3 = 'rgba(255,255,255,' + this.alpha + ')';
			ctx04.clearRect(0, 0, container0.width(), container0.height());
			ctx04.fillStyle = 'rgba(255,0,0,1)';
			ctx04.fillRect(0, 0, centerX0 * 2, centerY0 * 2 * 0.6);
			ctx04.fillStyle = rgba3;
			ctx04.fillText(this.text, this.x, this.y);
		}
	};

	var line0 = {
		width: drawLineWidth0,
		x1: centerX0,
		x2: centerX0,
		y: centerY0 * 2 * 0.6,

		draw: function () {
			this.x1 = centerX0 - this.width / 2;
			this.x2 = centerX0 + this.width / 2;
			ctx05.beginPath();
			ctx05.moveTo(this.x1, this.y);
			ctx05.lineTo(this.x2, this.y);
			ctx05.closePath();
			ctx05.stroke();
		}
	};

	var line02 = {
		x1: centerX0 - drawLineWidth0 / 2,
		x2: centerX0 + drawLineWidth0 / 2,
		y: centerY0 * 2 * 0.6,

		draw: function () {
			ctx06.clearRect(0, 0, container0.width(), container0.height());
			ctx06.beginPath();
			ctx06.moveTo(this.x1, this.y);
			ctx06.lineTo(this.x2, this.y);
			ctx06.closePath();
			ctx06.stroke();
		}
	};


	var mathPiTwo = Math.PI / 2;

	var circleGrey = {
		drawV: 2,
		initAngle: 0,
		radius: 200,
		color: 'blue',

		draw: function () {
			ctx00.beginPath();
			ctx00.arc(centerX0, centerY0, centerX0 - lineWidth / 2, -mathPiTwo, this.initAngle * Math.PI / 180 - mathPiTwo, false);

			ctx00.stroke();
		}
	};

	var circle = {
		drawV: 0,
		initAngle: 0,
		radius: 200,
		color: 'blue',
		alpha: 0,
		draw: function () {
			gradient0 = ctx01.createLinearGradient(0, 0, container0.width(), container0.height());
			gradient0.addColorStop(0, "rgba(0, 183, 225," + this.alpha + ")"); //blue
			gradient0.addColorStop(1, "rgba(30, 52, 129," + this.alpha + ")"); // green
			ctx01.strokeStyle = gradient0;
			ctx01.beginPath();
			ctx01.arc(centerX0, centerY0, centerX0 - lineWidth / 2, -mathPiTwo, this.initAngle * Math.PI / 180 - mathPiTwo, false);
			ctx01.stroke();
		}
	};
}
function drawGreyCircle() {

	ctx00.clearRect(0, 0, container0.width(), container0.height());

	circleGrey.draw();


	circleGrey.drawV *= 0.99;
	circleGrey.drawV += 0.25;
	circleGrey.initAngle += circleGrey.drawV;
	if (circleGrey.initAngle >= 380) {
		circleGrey.drawV = 4;
		timerGrayCercle = TweenMax.ticker.removeEventListener("tick", drawGreyCircle);
	}
	else {
		timerGrayCercle = TweenMax.ticker.addEventListener("tick", drawGreyCircle);
	}
}

function reverseDraw() {

	ctx01.clearRect(0, 0, container0.width(), container0.height());

	circle.draw();
	circle.drawV *= 0.99;
	circle.drawV += 0.1;
	circle.initAngle -= circle.drawV;

	if (circle.initAngle <= 360 * per / 100) {
		timerGrayCercleReverse = TweenMax.ticker.removeEventListener("tick", reverseDraw);
	}
	else {
		timerGrayCercleReverse = TweenMax.ticker.addEventListener("tick", reverseDraw);
	}
}

function drawTexts() {
	text1.draw();
	text2.draw();
	text3.draw();
	line0.draw();
}

function stopDrawingTexts() {
	timerDrawingTexts = TweenMax.ticker.removeEventListener("tick", drawTexts);
}

function animateElapsed() {

	currentDate = new Date();

	var e = Math.floor(endDate.getTime() / (1000 * 60 * 60 * 24));
	var c = Math.floor(currentDate.getTime() / (1000 * 60 * 60 * 24));

	elapsed = e - c >= 0 ? e - c : 0;

	per = Math.max(0, 100 - Math.floor(elapsed / 731 * 100));

	currVal = obj.getTextVal();
	endVal = parseInt(elapsed, 10);

	timerDrawingTexts = TweenMax.to(obj, 2 * (100 - per) / 100, {
		setTextVal: endVal,
		ease: TweenMax.Quart.easeInOut,
		onComplete: stopDrawingTexts
	});
}

function startCounting() {
	clearTimeout(reverseID);

	obj.setTextVal(0);
	animateElapsed();
	reverseDraw();
}

function drawCircle2Fade() {
	ctx01.clearRect(0, 0, container0.width(), container0.height());

	circle.initAngle = 360;
	circle.draw();

	circle.drawV *= 0.1;
	circle.drawV += 0.05;
	circle.alpha += circle.drawV;

	if (circle.alpha >= 1) {

		circle.drawV = 2;
		circle.initAngle = 360;
		timerFadeCercle = TweenMax.ticker.removeEventListener("tick", drawCircle2Fade);
		reverseID = setTimeout(startCounting, 500);
	} else {
		timerFadeCercle = TweenMax.ticker.addEventListener("tick", drawCircle2Fade);
	}
}

function drawLine02() {
	line02.draw();
}

function stopDrawLine2() {
	timerLineTwoCercle = TweenMax.ticker.removeEventListener("tick", drawLine02);
}

function initLine02() {
	timerLineTwoCercle = TweenMax.to(line02, 0.6, {
		x1: centerX0 + drawLineWidth0 / 2,
		delay: 0.6,
		repeat: 1,
		repeatDelay: 0.5,
		ease: TweenMax.Power2.easeInOut
	});
	timerLineTwoCercle = TweenMax.from(line02, 1.2, {
		x2: centerX0 - drawLineWidth0 / 2,
		delay: 0,
		repeat: 1,
		repeatDelay: 0.5,
		ease: TweenMax.Power2.easeInOut,
		onComplete: stopDrawLine2
	});
	timerLineTwoCercle = TweenMax.ticker.addEventListener("tick", drawLine02);
}

function settingCanvas0() {

	ctx00.shadowBlur = 6;
	ctx00.shadowColor = "rgba(100, 100, 100,.6)";
	ctx00.lineCap = "round";
	ctx00.strokeStyle = "rgba(255,255,255,.2)";
	ctx00.lineWidth = lineWidth;

	ctx01.lineCap = "round";
	ctx01.strokeStyle = gradient0;
	ctx01.lineWidth = lineWidth;

	ctx05.lineCap = "round";
	ctx05.strokeStyle = "rgba(255,255,255,.2)";
	ctx05.lineWidth = 1;

	ctx06.lineCap = "round";
	ctx06.strokeStyle = "rgba(175,175,175,1)";
	ctx06.lineWidth = 1;

	ctx02.globalCompositeOperation = "source-out";
	ctx02.font = fontSize * centerX0 * 2 / 776 + 'px' + ' maaxmedium';
	ctx02.textAlign = 'center';
	ctx02.textBaseline = "middle";

	ctx03.globalCompositeOperation = "source-out";
	ctx03.font = fontSize * centerX0 * 2 / 776 + 'px' + ' maaxbold';
	ctx03.textAlign = 'center';
	ctx03.textBaseline = "middle";

	ctx04.globalCompositeOperation = "source-out";
	ctx04.font = fontSize * centerX0 * 2 / 776 + 'px' + ' maaxmedium';
	ctx04.textAlign = 'center';
	ctx04.textBaseline = "middle";
}

function init0() {
	settingCanvas0();

	timerLineTwoCercle = TweenMax.from(line0, 0.6, {
		width: 0,
		delay: 0,
		ease: TweenMax.Quart.easeInOut,
		onComplete: initLine02
	});

	timerText01 = TweenMax.from(text1, 1, {
		y: centerY0 * 2 * 0.65,
		alpha: 1,
		delay: 0.4,
		ease: TweenMax.Power2.easeOut
	});
	timerText02 = TweenMax.from(text2, 1, {
		y: centerY0 * 2 * 0.75,
		alpha: 1,
		delay: 0.5,
		ease: TweenMax.Power2.easeOut
	});
	timerText03 = TweenMax.from(text3, 1, {
		y: centerY0 * 2 * 0.51,
		alpha: 1,
		delay: 0.5,
		ease: TweenMax.Power2.easeOut,
		onComplete: drawCircle2Fade
	});

	timerDrawingTexts = TweenMax.ticker.addEventListener("tick", drawTexts);

	timerGrayCercle = TweenMax.ticker.addEventListener("tick", drawGreyCircle);
}


function resizeCanvasCounter() {

	if (jQuery(window).get(0).innerWidth > jQuery(window).get(0).innerHeight) {

		var heightCount = jQuery(window).get(0).innerHeight * 0.8;
		if (jQuery(window).get(0).innerHeight <= 700) {
			var ratioK = jQuery(window).get(0).innerHeight / jQuery(window).get(0).innerWidth;
			ratioK = ratioK + 0.15;
			heightCount = jQuery(window).get(0).innerHeight * ratioK;
		}
		container0.width(heightCount);
		containerGlobal.width(heightCount);

		container0.height(heightCount);
		containerGlobal.height(heightCount);

		var correctTop = (jQuery(window).get(0).innerHeight - heightCount) / 2;

		var correctLeft = (jQuery(window).get(0).innerWidth - heightCount) / 2;
		containerGlobal.css('margin-top', correctTop + 'px');
		containerGlobal.css('margin-left', correctLeft + 'px');

	} else {
		var widthCount = jQuery(window).get(0).innerWidth * 0.4;
		container0.width(widthCount);
		container0.height(widthCount);

		containerGlobal.width(widthCount);
		containerGlobal.height(widthCount);

		var correctTop2 = (jQuery(window).get(0).innerHeight - widthCount) / 2;

		var correctLeft2 = (jQuery(window).get(0).innerWidth - widthCount) / 2;
		containerGlobal.css('margin-top', correctTop2 + 'px');
		containerGlobal.css('margin-left', correctLeft2 + 'px');
	}


	layer00.attr("width", container0.width());
	layer00.attr("height", container0.height());
	layer01.attr("width", container0.width());
	layer01.attr("height", container0.height());
	layer02.attr("width", container0.width());
	layer02.attr("height", container0.height());
	layer03.attr("width", container0.width());
	layer03.attr("height", container0.height());
	layer04.attr("width", container0.width());
	layer04.attr("height", container0.height());
	layer05.attr("width", container0.width());
	layer05.attr("height", container0.height());
	layer06.attr("width", container0.width());
	layer06.attr("height", container0.height());

	centerX0 = container0.width() / 2;
	centerY0 = container0.height() / 2;

	per = 0;

	numDays = 0;

	drawLineWidth0 = container0.width() * 0.50;

	gradient0 = ctx01.createLinearGradient(0, 0, container0.width(), container0.height());
	gradient0.addColorStop(0, "rgba(0, 183, 225, 1)"); //blue
	gradient0.addColorStop(1, "rgba(30, 52, 129, 1)"); // green

	text1.x = centerX0;
	text1.y = centerY0 * 2 * 0.43;
	text1.alpha = 1;


	text2.x = centerX0;
	text2.y = centerY0 * 2 * 0.55;
	text2.alpha = 1;

	text3.text = 'j - 0';
	text3.x = centerX0;
	text3.y = centerY0 * 2 * 0.66;
	text3.alpha = 1;

	line0.width = drawLineWidth0;
	line0.x1 = centerX0;
	line0.x2 = centerX0;
	line0.y = centerY0 * 2 * 0.6;

	line02.x1 = centerX0 - drawLineWidth0 / 2;
	line02.x2 = centerX0 + drawLineWidth0 / 2;
	line02.y = centerY0 * 2 * 0.6;

	circleGrey.drawV = 2;
	circleGrey.initAngle = 0;
	circleGrey.radius = 200;
	circleGrey.color = 'blue';

	circle.drawV = 0;
	circle.initAngle = 0;
	circle.radius = 200;
	circle.color = 'blue';
	circle.alpha = 0;

	timerGrayCercle = TweenMax.ticker.removeEventListener("tick", drawGreyCircle);
	timerGrayCercleReverse = TweenMax.ticker.removeEventListener("tick", reverseDraw);
	timerFadeCercle = TweenMax.ticker.removeEventListener("tick", drawCircle2Fade);

	timerLineTwoCercle = TweenMax.ticker.removeEventListener("tick", drawLine02);


	timerDrawingTexts = TweenMax.ticker.removeEventListener("tick", drawTexts);
	clearTimeout(reverseID);

	if (timerGrayCercleReverse) {
		timerGrayCercleReverse.kill();
	}

	if (timerDrawingTexts) {
		timerDrawingTexts.kill();
	}

	if (timerText01) {
		timerText01.kill();
	}
	if (timerText02) {
		timerText02.kill();
	}
	if (timerText03) {
		timerText03.kill();
	}
	if (timerFadeCercle) {
		timerFadeCercle.kill();
	}
	if (timerLineTwoCercle) {
		timerLineTwoCercle.kill();
	}

	init0();
}


var mouseContainer = jQuery("#mouseContainer");

if (jQuery("#mouseContainer").length) {
	var mouseStroke = document.querySelector('#mouseStroke');
	var mouseStrokePath = document.querySelector('#mouseStroke path');
	var mouseArrowPath = document.querySelector('#mouseArrow path');
	var mouseGradient = document.querySelector('#mouseGradient');
	var mouseGradientRect = document.querySelector('#mouseGradient rect');

	var mousePathLength = mouseStrokePath.getTotalLength();

	var tMousePath;
	var tMouseArrow;
	var tMouseMask;
	var tmouseStrokeHide;


	var reminderID;
	var animating = true;
	var animatingHover = false;
	var outArea = true;
	var remiderDelay = 2000;

	var Snap = require("snapsvg");

	var sMouseStroke = Snap("#mouseStroke");
	sMouseStroke.select("path");

	var sMouseArrow = Snap("#mouseArrow");
	var sMouseArrowPath = sMouseArrow.select("path");

	var sMouseGradient = Snap("#mouseGradient");
	sMouseGradient.select("path");

	var timerLoadingScroll;
}


function arrowCallBack() {
	clearTimeout(reminderID);
	reminderID = setTimeout(function () {
		tMouseArrow = TweenMax.fromTo(mouseArrowPath, 0.5, {
			opacity: 1,
			transform: "matrix(1, 0, 0, 1, 0, 0)"
		}, {
			opacity: 0,
			transform: "matrix(1, 0, 0, 1, 0, 40)",
			delay: 0,
			ease: TweenMax.Quart.easeOut,
			onComplete: arrowCallBack
		});
	}, remiderDelay);
	tMouseArrow = TweenMax.fromTo(mouseArrowPath, 0.5, {
		opacity: 0.6,
		transform: "matrix(1, 0, 0, 1, 0, -80)"
	}, {opacity: 1, transform: "matrix(1, 0, 0, 1, 0, 0)", delay: 0, ease: TweenMax.Quart.easeOut});
}

function mouseReminder() {
	tMouseArrow = TweenMax.fromTo(mouseArrowPath, 0.5, {opacity: 1, transform: "matrix(1, 0, 0, 1, 0, 0)"}, {
		opacity: 0,
		transform: "matrix(1, 0, 0, 1, 0, 40)",
		delay: 0,
		ease: TweenMax.Quart.easeOut,
		onComplete: arrowCallBack
	});
}

function setMouseReminder() {
	animating = false;
	reminderID = setTimeout(mouseReminder, remiderDelay);
}
function drawMouseIcon() {
	animating = true;
	tMousePath = TweenMax.to(mouseStrokePath, 1, {strokeDashoffset: 0, delay: 0, ease: TweenMax.Quart.easeInOut});
	tMouseArrow = TweenMax.fromTo(mouseArrowPath, 1, {opacity: 0, transform: "matrix(1, 0, 0, 1, 0, 40)"}, {
		opacity: 1,
		transform: "matrix(1, 0, 0, 1, 0, 0)",
		delay: 0.5,
		ease: TweenMax.Quart.easeOut,
		onComplete: setMouseReminder
	});
}
function overMouseIcon() {
	if (!animating && outArea) {
		animating = true;
		outArea = false;
		clearTimeout(reminderID);
		tMousePath.kill();
		tMouseArrow.kill();

		tmouseStrokeHide = TweenMax.to(mouseStroke, 0.5, {
			opacity: 0, delay: 0, ease: TweenMax.Quart.easeInOut, onComplete: function () {
				mouseStroke.style.opacity = 1;
				mouseStrokePath.style.strokeDashoffset = mousePathLength;
				drawMouseIcon();
			}
		});
	}
}


function leaveMouseIcon() {
	outArea = true;
}

function clickMouseIcon() {

	if (!animatingHover) {
		outArea = false;

		animating = true;
		animatingHover = true;

		mouseGradientRect.setAttribute('transform', 'matrix(1, 0, 0, 1, 0, -232)');

		mouseGradient.style.opacity = 1;
		mouseGradient.style.zIndex = 9;

		mouseStrokePath.style.strokeDashoffset = 0;

		clearTimeout(reminderID);

		tMousePath.kill();
		tMouseArrow.kill();

		sMouseArrowPath.attr({transform: "matrix(1, 0, 0, 1, 0, 0)", opacity: 1});

		jQuery('#mouseArrow path').css({transform: "matrix(1, 0, 0, 1, 0, 0)", opacity: 1});

		if (tMouseMask) {
			tMouseMask.kill();
		}

		var temp = {transform: "matrix(1, 0, 0, 1, 0, 0)"};


		tMouseMask = TweenMax.fromTo(temp, 0.5, {transform: "matrix(1, 0, 0, 1, 0, -232)"}, {
			transform: "matrix(1, 0, 0, 1, 0, 0)", delay: 0, ease: TweenMax.Quart.easeIn,
			onUpdate: function () {
				mouseGradientRect.setAttribute('transform', temp.transform);
			},
			onComplete: function () {
				tMouseMask.kill();
				tMouseMask = TweenMax.fromTo(temp, 0.5, {transform: "matrix(1, 0, 0, 1, 0, 0)"}, {
					transform: "matrix(1, 0, 0, 1, 0, 232)",
					delay: 0.4,
					ease: TweenMax.Quart.easeOut,
					onUpdate: function () {
						mouseGradientRect.setAttribute('transform', temp.transform);
					},
					onComplete: function () {
						animatingHover = false;
						mouseGradient.style.zIndex = 0;
						mouseGradient.style.opacity = 0;

						timerLoadingScroll = setTimeout(function () {
							clickMouseIcon();
						}, 600);
					}
				});
			}
		});
	}
}


function initMouseIcon() {
	mouseStrokePath.getBoundingClientRect();
	mouseGradientRect.getBoundingClientRect();
	mouseGradient.getBoundingClientRect();

	mouseStrokePath.style.strokeDasharray = mousePathLength + ' ' + mousePathLength;
	mouseStrokePath.style.strokeDashoffset = mousePathLength;

	mouseGradient.style.opacity = 0;
	mouseGradient.style.zIndex = 0;

	/* disable use function */
	if(false){
		mouseContainer.on("mouseover", overMouseIcon);
		mouseContainer.on("mouseleave", leaveMouseIcon);
	}


	mouseContainer.css({visibility: "visible"});

	drawMouseIcon();
}
function killClickMouseIcon() {
	animatingHover = false;
	if (tMouseMask) {
		tMouseMask.kill();
	}
	clearTimeout(timerLoadingScroll);
	animating = false;
	initMouseIcon();
}

/*require('browsernizr/test/video/autoplay');
 require('browsernizr');
 var Modernizr = require('browsernizr');*/

require("../vendor/jquery.videobackground");
require("../vendor/okvideo");


require("./../../../bower_components/history.js/scripts/bundled/html4+html5/jquery.history.js");
var History = window.History;

var firstTime = true;


var isAutoplaySupported = function (callback) {
	// Is the callback a function?
	if (typeof callback !== 'function') {
		return false;
	}

	if (!sessionStorage.autoplaySupported) {
		// Create video element to test autoplay
		var video = document.createElement('video');
		video.autoplay = true;
		video.src = 'data:video/mp4;base64,';
		video.load();
		video.style.display = 'none';
		video.playing = false;
		video.play();
		// Check if video plays
		video.onplay = function () {
			this.playing = true;
		};

		video.oncanplay = function () {
			if (video.playing) {
				sessionStorage.autoplaySupported = 'true';
				callback(true);
			} else {
				sessionStorage.autoplaySupported = 'false';
				callback(false);
			}
		};
	} else {
		if (sessionStorage.autoplaySupported === 'true') {
			callback(true);
		} else {
			callback(false);
		}
	}
};

var TweenMax = require("./../../../bower_components/gsap/src/minified/TweenMax.min");
/* right swiper */

var navRightContainer = jQuery("#navRightContainer");

var navRightStroke = document.querySelector('#navRightStroke');


var navRightStrokePath = document.querySelector('#navRightStroke path');
var navRightGrey = document.querySelector('#navRightStrokeGrey');
var navRightGreytRect = document.querySelector('#navRightStrokeGrey rect');
var navRightGradientRect = document.querySelector('#navRightGradient rect');

if (jQuery("#navRightStroke").length) {
	var navRightPathLength = navRightStrokePath.getTotalLength();
}
else {
	var navRightPathLength = 0;
}

var tnavRighMaskGrey;
var tnavRightMask;
var tnavRightPath;
var tnavRightStrokeHide;

var animatingNavRight = true;
var outAreaNavRight = true;
var loadingNavRight = false;
var navRightID;

var swiper_nav_time = 1600;


var temp2 = {transform: "matrix(0, 0, 0, 1, 20, 0)"};

function onUpdateHandler2() {
	navRightGreytRect.setAttribute('transform', temp2.transform);
}


var temp3 = {transform: "matrix(0, 0, 0, 1, 0, 0)"};

function onUpdateHandler3() {
	navRightGradientRect.setAttribute('transform', temp3.transform);
}

function drawnavRightIcon() {
	animatingNavRight = true;

	tnavRightPath = TweenMax.to(navRightStrokePath, 1, {
		strokeDashoffset: 0,
		delay: 0,
		ease: TweenMax.Quart.easeInOut,
		onComplete: function () {
			animatingNavRight = false;
		}
	});

}

function overnavRightIcon() {
	if (!animatingNavRight && outAreaNavRight) {

		animatingNavRight = true;

		outAreaNavRight = false;
		tnavRightPath.kill();

		tnavRightStrokeHide = TweenMax.to(navRightStroke, 0.5, {
			opacity: 0, delay: 0, ease: TweenMax.Quart.easeInOut, onComplete: function () {
				navRightStroke.style.opacity = 1;
				navRightStrokePath.style.strokeDashoffset = navRightPathLength;
				drawnavRightIcon();

			}
		});
	}
}

function leavenavRightIcon() {
	outAreaNavRight = true;
}

function navRightLoadAnim() {

	if (tnavRightMask) {
		tnavRightMask.kill();
	}

	animatingNavRight = true;


	tnavRightMask = TweenMax.fromTo(temp3, 0.1, {transform: "matrix(0, 0, 0, 1, 0, 0)"}, {
		transform: "matrix(1, 0, 0, 1, 0, 0)",
		delay: 0,
		ease: TweenMax.Quart.easeIn,
		onUpdate: onUpdateHandler3,
		onComplete: function () {
			tnavRightMask.kill();
			tnavRightMask = TweenMax.fromTo(temp3, 0.4, {transform: "matrix(1, 0, 0, 1, 0, 0)"}, {
				transform: "matrix(0, 0, 0, 1, 65, 0)",
				delay: 0.5,
				ease: TweenMax.Quart.easeOut,
				onUpdate: onUpdateHandler3,
				onComplete: function () {
					navRightID = setTimeout(navRightLoadAnim, 200);
				}
			});
		}
	});

}


function navRightAnimKill() {
	animatingNavRight = false;
	loadingNavRight = false;
	navRightGrey.style.opacity = 1;
	if (tnavRighMaskGrey) {
		tnavRighMaskGrey.kill();
	}
	if (tnavRightMask) {
		tnavRightMask.kill();
	}
	navRightGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');
	navRightGradientRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 0, 0)');
	clearTimeout(navRightID);
}

function clicknavRightIcon() {
	if (!loadingNavRight) {
		animatingNavRight = true;
		if (tnavRighMaskGrey) {
			tnavRighMaskGrey.kill();
		}

		navRightGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');


		tnavRighMaskGrey = TweenMax.fromTo(temp2, 0.01, {transform: "matrix(0, 0, 0, 1, 20, 0)"}, {
			transform: "matrix(1, 0, 0, 1, 0, 0)",
			delay: 0,
			ease: TweenMax.Quart.easeInOut,
			onUpdate: onUpdateHandler2,
			onComplete: function () {
				tnavRighMaskGrey.kill();
				loadingNavRight = true;
				tnavRighMaskGrey = TweenMax.fromTo(navRightGrey, 0.2, {opacity: 1}, {
					opacity: 0.4,
					delay: 0,
					ease: TweenMax.Quart.easeInOut,
					onComplete: navRightLoadAnim
				});
			}
		});


		mySwiper.slideNext();


		setTimeout(function () {
			navRightAnimKill();
		}, swiper_nav_time);

	}
}

function initnavRightIcon() {

	navRightStroke.getBoundingClientRect();
	navRightStrokePath.style.strokeDasharray = navRightPathLength + ' ' + navRightPathLength;
	navRightStrokePath.style.strokeDashoffset = navRightPathLength;

	navRightContainer.on("click", clicknavRightIcon);

	/* disbale use function */
	if(false){
		navRightContainer.on("mouseover", overnavRightIcon);
		navRightContainer.on("mouseleave", leavenavRightIcon);
	}


	navRightGreytRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 20, 0)');
	navRightGradientRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 0, 0)');

	drawnavRightIcon();
}


