	navLeftGradientRect.setAttribute('transform', 'matrix(0, 0, 0, 1, 0, 0)');

	drawnavLeftIcon();

}

function loadBgVideoVimeo(){
	if (jQuery('.swiper-slide .bg-video').length > 0) {
		jQuery('.swiper-slide .bg-video').each(function (e) {
			var bgVideo = jQuery(this);
			var linkVimeoSection = bgVideo.data('link-video');

			bgVideo.okvideo({
				source: linkVimeoSection + "___" + e,
				volume: 0,
				loop: true,
				hd: true,
				adproof: true,
				annotations: false
			});
		});
	}
}

function loadVimeosVideo() {

	if (jQuery('.swiper-slide .video-background')) {
		jQuery('.swiper-slide .video-background').each(function (e) {
			if (jQuery('iframe', this).length === 0) {
				var linkVimeoSection = jQuery(this).data('link-video');
				if (linkVimeoSection) {
					jQuery(this).okvideo({
						source: linkVimeoSection + "___" + e,
						volume: 0,
						loop: true,
						hd: true,
						adproof: true,
						annotations: false
					});
				}
			}

		});
	}
}


module.exports = function () {
	loadBgVideoVimeo();
	if(jQuery('.hp-content').length){
		initMouseIcon();
	}


	// Instantiate the "swiper"
	if (jQuery('.intro-loader').length > 0 && !jQuery('.intro-loader').hasClass('hide')) {
		resizeCanvas();
	}

	if (jQuery('.intro-counter').length > 0 && !jQuery('.intro-counter').hasClass('hide')) {
		resizeCanvasCounter();
	}

	jQuery(window).resize(function () {
		if (jQuery('.intro-counter').length > 0 && !jQuery('.intro-counter').hasClass('hide')) {
			resizeCanvasCounter();
		}

		if (jQuery('.intro-loader').length > 0 && !jQuery('.intro-loader').hasClass('hide')) {
			resizeCanvas();
		}
	});


	var elem_video = jQuery('.video-background').eq(0);


	var autoplay = true;
	isAutoplaySupported(function (supported) {
		if (!supported) {
			autoplay = false;
		}
	});


	var firstSlider = 0;

	if (jQuery('#active-swiper').length) {
		var active_swiper_hash = jQuery('#active-swiper').attr('data-id');

		if (active_swiper_hash > 0) {
			var swiper_with_hash = jQuery('.hp-content .swiper-slide[data-hash="' + active_swiper_hash + '"]');
			var indexSwiper = jQuery(".hp-content .swiper-slide").index(swiper_with_hash);
			firstSlider = indexSwiper;
		}
	}

	if (!jQuery('#page').hasClass('hide')) {
		if (autoplay) {
			elem_video.empty();
			var linkVimeo1 = elem_video.data('link-video');
			if (linkVimeo1) {
				elem_video.okvideo({
					source: linkVimeo1,
					volume: 0,
					loop: true,
					hd: true,
					adproof: true,
					annotations: false,
					onReady: function () {

					}
				});
			}
			else {
				elem_video.videobackground({
					videoSource: [[elem_video.data('mpquatre'), 'video/mp4'],
						[elem_video.data('webm'), 'video/webm'],
						[elem_video.data('ogv'), 'video/ogg']],
					poster: elem_video.data('poster'),
					loop: true,
					loadedCallback: function () {

					}
				});
			}
		}
	}


	mySwiper = new Swiper('.swiper-container', {
		loop: true,
		parallax: false,
		hashnav: false,
		keyboardControl: true,
		//virtualTranslate; true,
		speed: 1200,
		initialSlide: firstSlider,
		onInit: function () {
			var hash = getActiveHash();
			var content = document.querySelector("#content-" + hash);
			velocity(content, {opacity: 1}, {visibility: "visible", display: "block"});

			jQuery("#swiper-element-" + hash).removeClass("element-hidden");
			jQuery("#swiper-element-" + hash).addClass("element-active");


			var hash_title = jQuery(".swiper-container .swiper-slide[data-hash='" + hash + "']").attr('data-title');

			jQuery('.hp-header .header-text').html(hash_title);

			if (jQuery(window).innerWidth() < 768) {
				jQuery(".video-num-" + hash).addClass("element-active");
				jQuery(".video-to-play-" + hash).addClass("element-active");
			}

			jQuery(".swiper-container .swiper-slide[data-hash='" + hash + "']").addClass('active-animation');

		   // var href = jQuery(".hp-content .swiper-slide-active").attr("data-url");

			//var cible = jQuery("#content-" + hash);

			//ajaxLoadBlock(href, hash, cible)
			if (jQuery("#swiper-element-" + getActiveHash()).find('.event-video').length > 0) {
				jQuery('.intro-counter').removeClass('hide');
				jQuery('.intro-counter').css('z-index', 204);
				resizeCanvasCounter();
				jQuery('.intro-counter canvas').addClass('hide').hide();
				jQuery('#layer00').removeClass('hide').show();
				jQuery('#layer01').removeClass('hide').show();

			} else {
				jQuery('.intro-counter').addClass('hide');
				jQuery('.intro-counter').css('z-index', 4);
				jQuery('#layer00').addClass('hide').hide();
				jQuery('#layer01').addClass('hide').hide();
			}
			jQuery(".hp-content .swiper-slide-active").addClass('pre-loaded');

		},
		onSliderMove: function(){
			disableCustumScroll();
			startSwiper1 = true;
		},
		onTouchEnd: function(){

			setTimeout(function(){
				resizeCustumScroll();
				jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery(".site-content"), {scrollInertia: 0});
				startSwiper1 = false;
			}, 300);
		}
	});


	function getTransform(el) {
		return parseInt(el.css('transform').split(',')[4], 10);
	}

	var isTouchDevice = function () {
		return 'ontouchstart' in window || 'onmsgesturechange' in window;
	};

	if (!isTouchDevice()) {

		jQuery('.hp-content .swiper-button-prev').mouseenter(function () {

			// si animation slider terminer

			if (!isSwiperAnimating) {


				var hash = document.querySelector(".swiper-container .swiper-slide-active").getAttribute("data-swiper-slide-index");
				hash = parseInt(hash, 10) - 1;
				var swiperLenght = mySwiper.slides.length - 3;
				if (hash < 0) {
					hash = swiperLenght;
				}
				var swiperWrapp = jQuery('.hp-content .swiper-wrapper');

				var transform = mySwiper.translate + swiperMovePxHover;

				setTimeout(function () {
					swiperWrapp.css({
						'transform': 'translate3d(' + transform + 'px, 0px, 0px)',
						"transition-duration": "500ms"
					});
				}, 100);

			}
		});

		jQuery('.hp-content .swiper-button-prev').mouseleave(function () {
			if (!isSwiperAnimating) {
				var swiperWrapp = jQuery('.hp-content .swiper-wrapper');
				swiperWrapp.css({
					'transform': 'translate3d(' + mySwiper.translate + 'px, 0px, 0px)',
					"transition-duration": "500ms"
				});
				setTimeout(function () {
					swiperWrapp.css({"transition-duration": "0"});
				}, 1200);
			}

		});
		jQuery('.hp-content .swiper-button-next ').mouseenter(function () {
			// si animation slider terminer
			if (!isSwiperAnimating) {

				var hash = document.querySelector(".swiper-container .swiper-slide-active").getAttribute("data-swiper-slide-index");
				hash = parseInt(hash, 10) + 1;
				var swiperLenght = mySwiper.slides.length - 3;
				if (hash > swiperLenght) {
					hash = 0;
				}

				var swiperWrapp = jQuery('.hp-content .swiper-wrapper');
				var transform = mySwiper.translate - swiperMovePxHover;
				setTimeout(function () {
					swiperWrapp.css({
						'transform': 'translate3d(' + transform + 'px, 0px, 0px)',
						"transition-duration": "500ms"
					});
				}, 100);
			}
		});

		jQuery('.hp-content .swiper-button-next ').mouseleave(function () {
			if (!isSwiperAnimating) {
				var swiperWrapp = jQuery('.hp-content .swiper-wrapper');
				swiperWrapp.css({
					'transform': 'translate3d(' + mySwiper.translate + 'px, 0px, 0px)',
					"transition-duration": "500ms"
				});
				setTimeout(function () {
					swiperWrapp.css({"transition-duration": "0"});
				}, 1200);
			}
		});
	}

	var swiperPreloaded = false;

	function transitionStart() {
		jQuery("#jo-wrapper-detail, #footer-wrapper").addClass("hide");
		disableCustumScroll();
		isSwiperAnimating = true;
		jQuery('.swiper-container .swiper-slide').removeClass('active-effect');

		jQuery(".hp-content .swiper-elements > div").removeClass("element-active").addClass("element-hidden");

		if (jQuery(window).innerWidth() < 768) {
			jQuery("#zone-mobile .element-active").removeClass("element-active");

		}

		jQuery('.hp-content').addClass('start-sliding');

		var elem_video_two = jQuery('.swiper-container .swiper-slide').eq(mySwiper.activeIndex).find('.video-background');
		if (elem_video_two.length > 0) {
			var windowHeight = jQuery(window).height();
			jQuery('.swiper-container .swiper-slide').eq(mySwiper.activeIndex).find('.video-background').css('height', windowHeight);
		}
		if (jQuery("#swiper-element-" + getActiveHash()).find('.event-video').length === 0) {
			jQuery('.intro-counter').addClass('hide');
			if (!jQuery('#header').hasClass('hide')) {
				jQuery('#layer00').addClass('hide').hide();
				jQuery('#layer01').addClass('hide').hide();
			}
		}

		var elem_video_vimeo = jQuery('.swiper-container .swiper-slide').eq(mySwiper.activeIndex).find('.bg-video iframe, .video-background iframe');
		if (elem_video_vimeo.length > 0) {
			elem_video_vimeo.css('visibility', 'visible');
		}

		if (!jQuery('.hp-content .swiper-slide-active').hasClass('pre-loaded')) {
			var hash = jQuery(".hp-content .swiper-slide-active").attr("data-hash");
			var href = jQuery(".hp-content .swiper-slide-active").attr("data-url");
			var cible = jQuery("#content-" + hash);
			ajaxLoadBlock(href, hash, cible);
		} else {
			swiperPreloaded = true;
		}


	}

	function updateQueryStringParameter(uri, key, value) {
		var re = new RegExp("([?&])" + key + "=.*?(&|$)", "i");
		var separator = uri.indexOf('?') !== -1 ? "&" : "?";
		if (uri.match(re)) {
			return uri.replace(re, '$1' + key + "=" + value + '$2');
		}
		else {
			return uri + separator + key + "=" + value;
		}
	}

	function transitionEnd() {


		jQuery('.swiper-container .swiper-slide').eq(mySwiper.activeIndex).addClass('active-effect');


		jQuery("#swiper-element-" + getActiveHash()).removeClass("element-hidden").addClass("element-active");
		if (jQuery(window).innerWidth() < 768) {
			jQuery("#zone-mobile .element-active").removeClass("element-active");
			jQuery(".video-num-" + getActiveHash()).addClass("element-active");
			jQuery(".video-to-play-" + getActiveHash()).addClass("element-active");
		}

		if (jQuery("#swiper-element-" + getActiveHash()).find('.event-video').length > 0) {
			jQuery('.intro-counter').removeClass('hide');
			jQuery('.intro-counter').css('z-index', 204);
			resizeCanvasCounter();
			jQuery('.intro-counter canvas').addClass('hide').hide();
			jQuery('#layer00').removeClass('hide').show();
			jQuery('#layer01').removeClass('hide').show();

		} else {
			jQuery('.intro-counter').addClass('hide');
			jQuery('.intro-counter').css('z-index', 4);
			jQuery('#layer00').addClass('hide').hide();
			jQuery('#layer01').addClass('hide').hide();
		}

		jQuery('.hp-content').removeClass('start-sliding');
		getTransform(jQuery('.hp-content .swiper-wrapper'));
		isSwiperAnimating = false;
		jQuery('.swiper-container .swiper-slide').removeClass('active-animation');
		jQuery(".swiper-container .swiper-slide[data-hash='" + getActiveHash() + "']").addClass('active-animation');


		resizeCustumScroll();


		var theSection = jQuery('.hp-content .swiper-slide-active').attr('data-url');
		var pathArray = location.href.split('/');
		var protocol = pathArray[0];
		var host = pathArray[2];
		var url = protocol + '//' + host;
		theSection = theSection.replace(url, '');

		jQuery('.side-menu .menu-name-main-menu li a text').attr("fill", "#b5b5b5");
		jQuery('.side-menu .menu-name-main-menu li a').removeClass('active').parent().removeClass('active-trail active');
		jQuery('.side-menu .menu-name-main-menu li a[href="' + theSection + '"]').addClass('active').parent().addClass('active-trail active');
		jQuery('.side-menu .menu-name-main-menu li a[href="' + theSection + '"] text').attr("fill", "#000");
		if (swiperPreloaded) {
			jQuery('#jo-wrapper-detail, #footer-wrapper').removeClass('hide');
			swiperPreloaded = false;
			if (clickTimelineCall) {
				jQuery('.ajax-link').click();
				clickTimelineCall = false;
			}
		}
		var hash_title = jQuery('.hp-content .swiper-slide-active').attr('data-title');
		jQuery('.hp-header .header-text').html(hash_title);

		var readSpeakerUrl = jQuery('.readspeaker-button-jo a.rsbtn_play').attr('href');
		var newUrl = jQuery('.swiper-slide-active').attr('data-url');

		var newReadSpeakerUrl = updateQueryStringParameter(readSpeakerUrl, 'url', encodeURIComponent(newUrl));

		jQuery('.readspeaker-button-jo a.rsbtn_play').attr('href', newReadSpeakerUrl);
		
		window.resetSpeaker();		
	}

	if(jQuery('.hide-event-menu .side-menu .event').length){
		jQuery('.hide-event-menu .side-menu .event').remove();
	}

	if (jQuery("#navRightContainer").length) {
		initnavRightIcon();
	}

	if (jQuery("#navLeftContainer").length) {
		initnavLeftIcon();
	}



	function escapAnimationIntro() {
		escapeAnim = true;
		clearTimeout(timeintro);
		velocity(jQuery('#page'), {translateY: 0}, {
			duration: 200,
			complete: function () {
				killPreloader();
				jQuery('.intro-loader').addClass('hide');
				jQuery('#page').removeClass('overlay');
				jQuery('#page').removeAttr('style');
			}
		});


		jQuery('.intro-counter').removeClass('hide');
		resizeCanvasCounter();
		loadVimeosVideo();
		var elemActiveSection = jQuery('.element-active');
		var elemActiveSectionGlobal = jQuery('.swiper-slide-active');
		elemActiveSection.removeClass('element-active');
		elemActiveSectionGlobal.removeClass('active-effect');

		if (elemActiveSection.find('.event-video').length > 0) {
			jQuery('#layer00').show();
			jQuery('#layer01').show();
			jQuery('#page').removeClass('overlay');
		}
		else {
			jQuery('#layer00').addClass('hide');
			jQuery('#layer01').addClass('hide');

			jQuery('.intro-counter').addClass('hide');
			jQuery('#page').removeClass('overlay');
		}

		jQuery('#layer02').hide();
		jQuery('#layer03').hide();
		jQuery('#layer04').hide();
		jQuery('#layer05').hide();
		jQuery('#layer06').hide();
		jQuery('#page').css('z-index', '3');
		jQuery('.intro-counter').css('z-index', '4');
		velocity(jQuery('.content-section'), 'fadeIn', {
			duration: 100,
			complete: function () {
				elemActiveSection.addClass('element-active');
				elemActiveSectionGlobal.addClass('active-effect');
			}
		});

	}


	if (jQuery('.passer-intro').length) {
		jQuery(document).on('click', '.passer-intro', function (e) {
			e.preventDefault();
			jQuery(this).addClass("hide");

			escapAnimationIntro();
			jQuery('.intro-counter').hide();
			jQuery('.site-content.is-front-page').css('height', "auto");
			jQuery('.site-content').parent().removeAttr('style');
			resizeCustumScroll();

		});

		setTimeout(function () {
			if (!jQuery('.passer-intro').hasClass("hide")) {
				jQuery('.passer-intro').click();
			}
		}, 11800);
	}

	var currentHash = getActiveHash();
	currentHash = jQuery(".swiper-container .swiper-slide[data-hash='" + currentHash + "']").attr('data-swiper-slide-index');
	currentHash = parseInt(currentHash, 10);

	var data_escape_intro = jQuery('#page-wrapper').attr('data-escape-intro');
	if(parseInt(data_escape_intro, 10) === 1){
		currentHash = 10;
	}

	if (jQuery('#jo-wrapper-detail').length && currentHash !== 0) {

		jQuery('.site-content').parent().removeAttr('style');
		jQuery('.intro-counter').hide();
	}

	if (currentHash !== 0) {

		if (autoplay) {
			elem_video.empty();
			var linkVimeo = elem_video.data('link-video');
			if (linkVimeo) {
				elem_video.okvideo({
					source: linkVimeo,
					volume: 0,
					loop: true,
					hd: true,
					adproof: true,
					annotations: false,
					onReady: function () {

					}
				});
			}
			else {
				elem_video.videobackground({
					videoSource: [[elem_video.data('mpquatre'), 'video/mp4'],
						[elem_video.data('webm'), 'video/webm'],
						[elem_video.data('ogv'), 'video/ogg']],
					poster: elem_video.data('poster'),
					loop: true,
					loadedCallback: function () {

					}
				});
			}
		}
		else {
			if (firstTime) {
				setTimeout(function () {
					if (!escapeAnim) {
						velocity(jQuery('#page'), {translateY: 0}, {
							duration: 0, easing: 'ease-out',
							complete: function () {
								killPreloader();
								jQuery('.intro-loader').addClass('hide');
							}
						});

						jQuery('#page').addClass('overlay');
						setTimeout(function () {
							jQuery('.intro-counter').removeClass('hide');
							resizeCanvasCounter();
							loadVimeosVideo();
						}, 0);
						timeintro = setTimeout(function () {
							var elemActiveSectionMobile = jQuery('.element-active');
							var elemActiveSectionGlobalMobile = jQuery('.swiper-slide-active');
							elemActiveSectionMobile.removeClass('element-active');
							elemActiveSectionGlobalMobile.removeClass('active-effect');

							if (elemActiveSectionGlobalMobile.find('.event-video').length > 0) {
								jQuery('#layer00').show();
								jQuery('#layer01').show();
							}
							else {
								jQuery('#layer00').addClass('hide');
								jQuery('#layer01').addClass('hide');

								jQuery('.intro-counter').addClass('hide');
								jQuery('#page').removeClass('overlay');
							}

							jQuery('#layer02').hide();
							jQuery('#layer03').hide();
							jQuery('#layer04').hide();
							jQuery('#layer05').hide();
							jQuery('#layer06').hide();

							jQuery('#page').css('z-index', '3');
							jQuery('.intro-counter').css('z-index', '4');
							jQuery('.passer-intro').addClass("hide");
							velocity(jQuery('.content-section'), 'fadeIn', {
								duration: 0,
								complete: function () {
									setTimeout(function () {
										elemActiveSectionMobile.addClass('element-active');
										elemActiveSectionGlobalMobile.addClass('active-effect');
									}, 0);
								}
							});
						}, 0);
					}
				}, 0);
			} //end firstTime

			firstTime = false;
		}


	} else {
		disableCustumScroll();
		jQuery('.content-section').addClass('hide');
		adjustImageSize();
		jQuery('.content-section').css('opacity', 0);
		velocity(jQuery('#page'), {translateY: jQuery(window).get(0).innerHeight},
			{
				duration: 0,
				complete: function () {
					jQuery('#page').removeClass('hide');
					mySwiper.onResize();
					adjustImageSize();
					jQuery('.content-section').css('opacity', 1);
					velocity(jQuery('.content-section'), 'fadeOut', {duration: 0});
					jQuery('.content-section').removeClass('hide');
				}
			}
		);

		setTimeout(function () {
			if (autoplay && firstSlider === 0) {
				elem_video.empty();
				var linkVimeo = elem_video.data('link-video');
				if (linkVimeo) {
					elem_video.okvideo({
						source: linkVimeo,
						volume: 0,
						loop: true,
						hd: true,
						adproof: true,
						annotations: false,
						onReady: function () {
							if (firstTime) {
								setTimeout(function () {
									if (!escapeAnim) {
										jQuery(this).videobackground('mute');
										velocity(jQuery('#page'), {translateY: 0}, {
											duration: 700, easing: 'ease-out',
											complete: function () {
												killPreloader();
												jQuery('.intro-loader').addClass('hide');
											}
										});

										jQuery('#page').addClass('overlay');
										setTimeout(function () {
											jQuery('.intro-counter').removeClass('hide');
											jQuery('.intro-counter').css('z-index', 204);
											resizeCanvasCounter();
//jQuery('.intro-counter canvas').addClass('hide').hide();
											jQuery('#layer00').removeClass('hide').show();
											jQuery('#layer01').removeClass('hide').show();
											//resizeCanvasCounter();
											loadVimeosVideo();
										}, 2000);

										timeintro = setTimeout(function () {
											var elemActiveSection = jQuery('.element-active');
											var elemActiveSectionGlobal = jQuery('.swiper-slide-active');
											elemActiveSection.removeClass('element-active');
											elemActiveSectionGlobal.removeClass('active-effect');
											setTimeout(function () {
												if (elemActiveSection.find('.event-video').length > 0) {
													jQuery('#layer00').show();
													jQuery('#layer01').show();
													jQuery('#page').removeClass('overlay');
												}
												else {
													jQuery('#layer00').addClass('hide');
													jQuery('#layer01').addClass('hide');

													jQuery('.intro-counter').addClass('hide');
													jQuery('#page').removeClass('overlay');
												}
											}, 2000);


											jQuery('#layer02').hide();
											jQuery('#layer03').hide();
											jQuery('#layer04').hide();
											jQuery('#layer05').hide();
											jQuery('#layer06').hide();

											jQuery('#page').css('z-index', '3');
											jQuery('.intro-counter').css('z-index', '4');
											jQuery('.passer-intro').addClass("hide");
											velocity(jQuery('.content-section'), 'fadeIn', {
												duration: 800,
												complete: function () {
													setTimeout(function () {
														elemActiveSection.addClass('element-active');
														elemActiveSectionGlobal.addClass('active-effect');
														jQuery('.site-content.is-front-page').css('height', "auto");
														jQuery('.site-content').parent().removeAttr('style');
														jQuery('#page').removeAttr('style');
														resizeCustumScroll();

													}, 800);
												}
											});

											/*jQuery('.site-content').parent().removeAttr('style');
											 jQuery('#page').removeAttr('style');
											 resizeCustumScroll();*/
											jQuery('#page').removeAttr('style');
										}, 7000);
									}

								}, 2000);

							} //end firstTime


							firstTime = false;
						}
					});
				}
				else {
					if (elem_video.data('mpquatre') !== null || elem_video.data('webm') !== null || elem_video.data('ogv') !== null) {
						elem_video.videobackground({
							videoSource: [[elem_video.data('mpquatre'), 'video/mp4'],
								[elem_video.data('webm'), 'video/webm'],
								[elem_video.data('ogv'), 'video/ogg']],
							poster: elem_video.data('poster'),
							loop: true,
							loadedCallback: function () {
								if (firstTime) {
									setTimeout(function () {
										if (!escapeAnim) {
											jQuery(this).videobackground('mute');
											velocity(jQuery('#page'), {translateY: 0}, {
												duration: 700, easing: 'ease-out',
												complete: function () {
													killPreloader();
													jQuery('.intro-loader').addClass('hide');
												}
											});

											jQuery('#page').addClass('overlay');
											setTimeout(function () {
												jQuery('.intro-counter').removeClass('hide');
												jQuery('.intro-counter').css('z-index', 204);
												resizeCanvasCounter();
												//jQuery('.intro-counter canvas').addClass('hide').hide();
												jQuery('#layer00').removeClass('hide').show();
												jQuery('#layer01').removeClass('hide').show();
												loadVimeosVideo();
											}, 2000);
											timeintro = setTimeout(function () {
												var elemActiveSection = jQuery('.element-active');
												var elemActiveSectionGlobal = jQuery('.swiper-slide-active');
												elemActiveSection.removeClass('element-active');
												elemActiveSectionGlobal.removeClass('active-effect');

												if (elemActiveSection.find('.event-video').length > 0) {
													jQuery('#layer00').show();
													jQuery('#layer01').show();
													jQuery('#page').removeClass('overlay');
												}
												else {
													jQuery('#layer00').addClass('hide');
													jQuery('#layer01').addClass('hide');

													jQuery('.intro-counter').addClass('hide');
													jQuery('#page').removeClass('overlay');
												}

												jQuery('#layer02').hide();
												jQuery('#layer03').hide();
												jQuery('#layer04').hide();
												jQuery('#layer05').hide();
												jQuery('#layer06').hide();

												jQuery('#page').css('z-index', '3');
												jQuery('.intro-counter').css('z-index', '4');
												jQuery('.passer-intro').addClass("hide");
												velocity(jQuery('.content-section'), 'fadeIn', {
													duration: 800,
													complete: function () {
														setTimeout(function () {
															elemActiveSection.addClass('element-active');
															elemActiveSectionGlobal.addClass('active-effect');
															jQuery('.site-content.is-front-page').css('height', "auto");
															jQuery('.site-content').parent().removeAttr('style');
															jQuery('#page').removeAttr('style');
															resizeCustumScroll();
														}, 800);
													}
												});

												jQuery('#page').removeAttr('style');
											}, 7000);

										}

									}, 2000);


								} //end firstTime


								firstTime = false;
							}
						});
					} else {
						if (firstTime) {
							jQuery('#layer00').addClass('hide');
							jQuery('#layer01').addClass('hide');

							setTimeout(function () {
								if (!escapeAnim) {
									velocity(jQuery('#page'), {translateY: 0}, {
										duration: 700, easing: 'ease-out',
										complete: function () {
											killPreloader();
											jQuery('.intro-loader').addClass('hide');
										}
									});

									jQuery('#page').addClass('overlay');
									setTimeout(function () {
										jQuery('.intro-counter').removeClass('hide');
										jQuery('.intro-counter').css('z-index', 204);
										jQuery('.intro-counter #container-counter canvas').removeClass('hide').show();

										resizeCanvasCounter();
									}, 2000);
									timeintro = setTimeout(function () {
										var elemActiveSection = jQuery('.element-active');
										var elemActiveSectionGlobal = jQuery('.swiper-slide-active');
										elemActiveSection.removeClass('element-active');
										elemActiveSectionGlobal.removeClass('active-effect');
										if (elemActiveSection.find('.event-video').length > 0) {
											jQuery('#layer00').show();
											jQuery('#layer01').show();
											jQuery('#page').removeClass('overlay');
										}
										else {
											jQuery('#layer00').addClass('hide');
											jQuery('#layer01').addClass('hide');

											jQuery('.intro-counter').addClass('hide');
											jQuery('#page').removeClass('overlay');
										}

										jQuery('#layer02').hide();
										jQuery('#layer03').hide();
										jQuery('#layer04').hide();
										jQuery('#layer05').hide();
										jQuery('#layer06').hide();

										jQuery('#page').css('z-index', '3');
										jQuery('.intro-counter').css('z-index', '4');
										jQuery('.passer-intro').addClass("hide");
										velocity(jQuery('.content-section'), 'fadeIn', {
											duration: 800,
											complete: function () {
												setTimeout(function () {
													elemActiveSection.addClass('element-active');
													elemActiveSectionGlobal.addClass('active-effect');
													jQuery('.site-content.is-front-page').css('height', "auto");
													jQuery('.site-content').parent().removeAttr('style');
													jQuery('#page').removeAttr('style');
													resizeCustumScroll();
												}, 800);
											}
										});

									}, 7000);

								}
							}, 2000);


						} //end firstTime
					}

				}
			}
			else {
				if (firstTime) {
					setTimeout(function () {
						if (!escapeAnim) {
							velocity(jQuery('#page'), {translateY: 0}, {
								duration: 700, easing: 'ease-out',
								complete: function () {
									killPreloader();
									jQuery('.intro-loader').addClass('hide');
								}
							});

							jQuery('#page').addClass('overlay');
							setTimeout(function () {
								jQuery('.intro-counter').removeClass('hide');
								jQuery('.intro-counter').css('z-index', 204);
								resizeCanvasCounter();
								//jQuery('.intro-counter canvas').addClass('hide').hide();
								jQuery('#layer00').removeClass('hide').show();
								jQuery('#layer01').removeClass('hide').show();
								loadVimeosVideo();
							}, 2000);
							timeintro = setTimeout(function () {
								var elemActiveSectionMobile = jQuery('.element-active');
								var elemActiveSectionGlobalMobile = jQuery('.swiper-slide-active');
								elemActiveSectionMobile.removeClass('element-active');
								elemActiveSectionGlobalMobile.removeClass('active-effect');

								if (elemActiveSectionGlobalMobile.find('.event-video').length > 0) {
									jQuery('#layer00').show();
									jQuery('#layer01').show();
								}
								else {
									jQuery('#layer00').addClass('hide');
									jQuery('#layer01').addClass('hide');

									jQuery('.intro-counter').addClass('hide');
									jQuery('#page').removeClass('overlay');
								}

								jQuery('#layer02').hide();
								jQuery('#layer03').hide();
								jQuery('#layer04').hide();
								jQuery('#layer05').hide();
								jQuery('#layer06').hide();

								jQuery('#page').css('z-index', '3');
								jQuery('.intro-counter').css('z-index', '4');
								jQuery('.passer-intro').addClass("hide");
								velocity(jQuery('.content-section'), 'fadeIn', {
									duration: 800,
									complete: function () {
										setTimeout(function () {
											elemActiveSectionMobile.addClass('element-active');
											elemActiveSectionGlobalMobile.addClass('active-effect');
											jQuery('.site-content.is-front-page').css('height', "auto");
											jQuery('.site-content').parent().removeAttr('style');
											jQuery('#page').removeAttr('style');
											resizeCustumScroll();
										}, 800);
									}
								});
							}, 7000);
						}
					}, 2000);
				} //end firstTime

				firstTime = false;
			}
		}, 1000);

	}


	if (jQuery('.menu-name-main-menu').length > 0) {
		jQuery('.side-menu .menu-name-main-menu .menu li a').each(function () {

			var thatLink = jQuery(this);
			thatLink.click(function (event) {
				event.preventDefault();
				jQuery('.hp-header').removeClass('nav-intern');
				jQuery('.side-menu').removeClass('nav-intern-m');
				jQuery('body').removeClass('block-n-1');
				var theLink = jQuery(this).attr('href');

				var theSection = jQuery('*[data-url="' + theLink + '"]');

				if (theSection.length > 0) {

					var theIndexSection = theSection.eq(0).data('swiper-slide-index').mySwiper.slideTo(theIndexSection);

				}


			});
		});
	}


	// afterChange()
	//
	// =============
	// show new active slide
	// hide previous slide

	function afterChange() {
		mySwiper.prevSlide = jQuery('.swiper-container .swiper-slide').eq(mySwiper.previousIndex).attr("data-hash");

		var content = document.querySelector("#content-" + getActiveHash());
		jQuery("#jo-wrapper-detail .content > div").hide();
		jQuery("#jo-wrapper-detail .content > div").find('.rs_read_this').addClass('rs_read_this_post').removeClass('rs_read_this');

		velocity(content, {opacity: 1}, {visibility: "visible", display: "block"});
		jQuery('body').addClass('hidden-bar');

		var currSection = jQuery('.swiper-container .swiper-slide').eq(mySwiper.activeIndex);		

		History.pushState({state: 'Section'}, currSection.data('title'), currSection.data('url'));

		jQuery('.cover-texts').removeClass('rs_read_this');
		jQuery('.element-active .cover-texts').addClass('rs_read_this');

		content.find('.rs_read_this_post').addClass('rs_read_this');
	}


	mySwiper.on("SlideChangeEnd", afterChange);
	mySwiper.on("TransitionStart", transitionStart);
	mySwiper.on("TransitionEnd", transitionEnd);


	adjustImageSize();

	var debbie = debounce(adjustImageSize, 100);
	window.addEventListener('resize', debbie, false);

	// on clic see news slide heritage
	jQuery(document).on('click', '#display-timeline', function () {
		jQuery('.hp-header.nav-intern').removeClass('nav-intern');
		jQuery('.side-menu').removeClass('nav-intern-m');
		jQuery('body').removeClass('block-n-1');
		jQuery('#page-wrapper').mCustomScrollbar("scrollTo", 'top', {
			scrollInertia: 0
		});

		//mySwiper.slideNext();

		mySwiper.slideTo(mySwiper.slides.length - 2, 400, true);
		clickTimelineCall = true;


	});
	jQuery(document).on('click', '.menu-name-main-menu li a', function (e) {
		if (jQuery('body').hasClass("front")) {
			e.preventDefault();
			jQuery('#page-wrapper').mCustomScrollbar("scrollTo", 'top', {
				scrollInertia: 0,
				callbacks: false
			});
			setTimeout(function () {
				disableCustumScroll();
			}, 300);

		}
		jQuery(".hp-header .burger").removeClass('active');
		jQuery(this).addClass('active');
		var active = jQuery(this).parent().index();
		active = active + 1;
		mySwiper.slideTo(active, 500, true);
		//transitionStart();
		setTimeout(function () {
			//afterChange();
			//transitionEnd();
			if (jQuery(".hp-header .burger").hasClass("button-active")) {
				jQuery(".hp-header .burger").click();
				jQuery(".to-top").addClass('hide');
				jQuery(".get-video-wrap").removeClass('hide');
				jQuery("body").on("touchmove", false);

			}

		}, 600);
	});
