},{"velocity-animate":8}],13:[function(require,module,exports){
// debounce()
// ==========
// Part of FullSix's micro library.
//
// Underscore's debounce().
//
// Usage:
//
//     var debounce = require("./f6/debounce");
//     var debounced = debounce(my_function, 100, false);
//     window.addEventListener('resize', debounced, false);
module.exports = function(func, wait, immediate) {
	var timeout;

	func();

	return function() {
		var context = this, args = arguments;
		var callNow = immediate && !timeout;

		var later = function() {
			timeout = null;
			if (!immediate) {
				func.apply(context, args);
			}
		};

		window.clearTimeout(timeout);

		timeout = window.setTimeout(later, wait);

		if (callNow) {
			func.apply(context, args);
		}
	};
};

},{}],14:[function(require,module,exports){
// ready()
// =======
// Part of FullSix's micro library.
//
// Cross-browser `$(document).ready()` alternative.
//
// Usage:
//
//     var ready = require("./f6/ready");
//     ready(function() {
//        // do something
//     });
module.exports = function ready(func) {
	if (document.readyState === 'interactive'){
		func();
	}
	if (document.addEventListener) {
		document.addEventListener('DOMContentLoaded', func, false);
	} else {
		document.attachEvent('onreadystatechange', function() {
			if (document.readyState === 'interactive') {
				func();
			}
		});
	}
};

