		var s = Snap('#'+thatSvg);

		var text = s.text(0, 40, thatText);
		var textbis = s.text(0, 40, thatText);

		text.attr({
			'fill': '#b5b5b5'
		});
		if(jQuery(this).hasClass('active')){
			text.attr({
				'fill': '#000'
			});
		}


		textbis.attr({
			'fill': '#b5b5b5'
		});


		var rect = s.rect(-500, -20, 300, 100);
		var g = s.gradient('l()#1e3481-#00b7ff');
		rect.attr({
			'fill': g
		});

		rect.attr({
		  mask : textbis
		});

		thatLink
			.click(function() {
				jQuery(".side-menu .menu-name-main-menu svg > text").attr('fill','#b5b5b5');
				jQuery(this).find('svg > text').attr('fill','#000');
			})
			.mouseenter(function() {
				/*if(i-1 > 0){
					jQuery('.side-menu .menu-name-main-menu .menu li a').eq(i-1).parent().addClass("menu-hover");
				}*/
				var thatElem = jQuery(this);
				var rectText = thatElem.find('svg rect');
				velocity(rectText, "stop", true);
				velocity(rectText, {opacity: 1}, {duration: 0, queue: false});
				velocity(rectText, {x: 0}, {easing: "easeOutSine", duration: 400, delay: 100, queue: false});
			})
			.mouseleave(function() {
				//jQuery('.side-menu .menu-name-main-menu .menu li a').parent().removeClass("menu-hover");
				var thatElem = jQuery(this);
				var rectText = thatElem.find('svg rect');
				velocity(rectText, "stop", true);
				velocity(rectText, {opacity: 0}, {duration: 400, queue: false});
				velocity(rectText, {x: -500}, {duration: 0, delay: 200, queue: false});
			});

	});
}

/* ajaxComlete */

function pageAjaxCalled(){
	 if(jQuery('.block-partenaires').length > 0){
		var width = jQuery('.block-partenaires .grid-item').width();

			jQuery('.block-partenaires .grid-item').each(function(){
				jQuery(this).css('height',width+"px");
			});

			jQuery(window).resize(function() {
				var width = jQuery('.block-partenaires .grid-item').width();

				jQuery('.block-partenaires .grid-item').each(function(){
					jQuery(this).css('height',width+"px");
				});
			});
	 }
	// ajax list des sites
	var load = false;
	if(jQuery('.navigation .map-list-cta a').length){
		jQuery(document).on('click', '.navigation .map-list-cta a', function(e){
			e.preventDefault();
			var href = jQuery(this).attr('href');
			var cible = jQuery('#map-ajax-content');
			if (load === false) {
				load = true;
				cible.css('opacity', 0.6);
				jQuery.ajax({
					type: 'POST',
					url: href,
					data: {
						'jo_ajax': 1
					},
					success: function (datarespo) {
						cible.removeAttr("style");
						var responseData = jQuery.parseJSON(datarespo);
						load = false;
						var dataHtml;
						jQuery.each( responseData, function( i, val ) {
							if(val.command === 'insert') {
								dataHtml = val.data;
							}
						});
						if (dataHtml) {
							cible.html(dataHtml);
						}
					}
				});
			}
		});
	}

	if(".site-olympe"){
		jQuery(document).on('click', ".site-olympe #carte-olympiques map[name=imagemap] area", function (e) {
			e.preventDefault();
			var href = jQuery(this).attr('href');
			jQuery(".site-olympe #carte-olympiques .map-carte-detail .map-detail").removeClass("active");
			jQuery(".site-olympe #carte-olympiques .map-carte-detail .map-detail[data-href='"+href+"']").eq(0).addClass("active");
		});
		jQuery(document).on('click', ".site-olympe #carte-paralympiques map[name=imagemap] area", function (e) {
			e.preventDefault();
			var href = jQuery(this).attr('href');
			jQuery(".site-olympe #carte-paralympiques .map-carte-detail .map-detail").removeClass("active");
			jQuery(".site-olympe #carte-paralympiques .map-carte-detail .map-detail[data-href='"+href+"']").eq(0).addClass("active");
		});
	}

	if(jQuery('.site-olympe').length > 0 ){
		jQuery(document).on('click','.site-olympe .img a.link', function(event){
			event.preventDefault();
			jQuery('.site-olympe .img.legend').addClass('animate');
			jQuery('.site-olympe .img').toggleClass('active');
			jQuery('.site-olympe .img.legend').toggleClass('desactive');

		});
	}

	if(jQuery('.site-olympe #cta #switch').length > 0) {
		var show_olympique = false;

		jQuery(document).on('change','#switch input',function() {

			jQuery('.site-olympe .img').removeClass('active');
			jQuery('.site-olympe .img').removeClass('animate');
			jQuery('.site-olympe .img.legend').addClass('desactive');
			jQuery('.site-olympe .img.carte').addClass('active');


			jQuery('#carte-paralympiques').toggleClass('active');
			jQuery('#carte-olympiques').toggleClass('active');
			jQuery('#switch label').toggleClass('gras');

			if(show_olympique){
				jQuery('#carte-sites').show();
				show_olympique = false;
			}else{
				jQuery('#carte-sites').hide();
				show_olympique = true;
			}

		});
	}

	// map close popin

	if(jQuery('.site-olympe .map-detail button').length){
		jQuery(document).on('click', '.site-olympe .map-detail button', function(){
			jQuery(this).parent().parent().removeClass("active");
		});
	}






	if(jQuery(".timeline-title").length){
		jQuery(".timeline-title").each(function(i){
			var thatLink = jQuery(this);

			var thatSvg = 'svg-menu-'+i;
			var thatText = thatLink.html();

			if(!jQuery('svg',this).length){
				thatLink.empty();



				thatLink.html('<svg version="1.1" id="'+thatSvg+'" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="200px" height="70px"></svg></svg>');

				var s = Snap('#'+thatSvg);

				var text = s.text(100, 60, thatText);
				text.attr({"text-anchor":"middle"});
				var textbis = s.text(100, 60, thatText);
				textbis.attr({"text-anchor":"middle"});

				text.attr({
					'fill': '#b5b5b5'
				});

				textbis.attr({
					'fill': '#b5b5b5'
				});


				var rect = s.rect(-500, 0, 200, 70);
				var g = s.gradient('l()#1e3481-#00b7ff');
				rect.attr({
					'fill': g
				});

				rect.attr({
					mask : textbis
				});

				/*thatLink
				 .mouseenter(function() {

				 })
				 .mouseleave(function() {
				 var thatElem = jQuery(this);
				 var rectText = thatElem.find('svg rect');
				 velocity(rectText, "stop", true);
				 velocity(rectText, {opacity: 0}, {duration: 400, queue: false});
				 velocity(rectText, {x: -500}, {duration: 0, delay: 200, queue: false});
				 });*/

				var thatElem = jQuery(this);
				var rectText = thatElem.find('svg rect');
				velocity(rectText, "stop", true);
				velocity(rectText, {opacity: 1}, {duration: 0, delay: 0, queue: false});
				velocity(rectText, {x: 0}, {duration: 0, delay: 0, queue: false});
			}


		});

	}
}
pageAjaxCalled();
jQuery( document ).ajaxComplete(function() {
	pageAjaxCalled();
});


module.exports = function(){
	var TweenMax = require("./../../../bower_components/gsap/src/minified/TweenMax.min");
	var Snap = require("snapsvg");
	var scSVGSize = 56,
		scCentre = scSVGSize/2,
		scRadius = (scSVGSize-8)/2,
		scSVG = Snap('#smallCountdownSvg'),
		smallCountdownSvg = jQuery('#smallCountdownSvg'),
		path = "",
		scArc = scSVG.path(path),
		startY = scCentre-scRadius;


	function drawSmallCountdown(percent) {
		if(percent>=1){
			percent = 0.99;
		}
		var endpoint = percent*360;

		var greyCircle = scSVG.circle(scCentre, scCentre, scRadius);
		// By default its black, lets change its attributes
		greyCircle.attr({
			fill: "none",
			stroke: "rgba(255,255,255,0.2)",
			strokeWidth: 8
		});

		scArc.remove();
		var ep = endpoint,
			dr = ep-90,

			radians = Math.PI*dr/180,
			endx = scCentre + scRadius*Math.cos(radians),
			endy = scCentre + scRadius * Math.sin(radians),
			largeArc = ep>180 ? 1 : 0;
		path = "M"+scCentre+","+startY+" A"+scRadius+","+scRadius+" 0 "+largeArc+",1 "+endx+","+endy;

		scArc = scSVG.path(path);
		scArc.attr({
			stroke: 'L()#1e3481-#00b7ff',
			fill: 'none',
			strokeWidth: 8,
			strokeLinecap:'round'
		});
		TweenMax.fromTo(smallCountdownSvg, 1, {opacity:0},{opacity:1, ease: TweenMax.Quart.easeInOut});
	}



	var data_count = jQuery('.hp-header .compte-rebours').attr('data-count');

	drawSmallCountdown(data_count);
};

if(jQuery('.form-benevoles').length){
	var current_form_item;
	jQuery('.form-benevoles .form-item').each(function(){
		current_form_item = jQuery(this);
		if(current_form_item.hasClass('grid-item')){
			console.log('ok');
			current_form_item.find("label").addClass("benevoles-label").insertAfter(current_form_item.find('input'));
			//jQuery('label', this).insertAfter('input', this);
			//jQuery('label', this).addClass('test'+e);
		}
	});

	jQuery('.form-benevoles .form-item input').focusin(function() {
		current_form_item = jQuery(this);
		current_form_item.addClass('active');
		current_form_item.parent().find('.benevoles-label').addClass("active");
	});

	jQuery('.form-benevoles .form-item input').focusout(function() {
		current_form_item = jQuery(this);
		if(current_form_item.val()){
			current_form_item.addClass('active');
			current_form_item.parent().find('.benevoles-label').addClass("active");
		}else{
			current_form_item.removeClass('active');
			current_form_item.parent().find('.benevoles-label').removeClass("active");
		}
	});
}

if(jQuery('.shortlinks').length){
	var shortlink = jQuery('.shortlinks a');
	shortlink.on('focus', function(){
		jQuery('.shortlinks').addClass('active');
	});

	shortlink.on('blur', function(){
		jQuery('.shortlinks').removeClass('active');
	});
}




},{"./../../../bower_components/gsap/src/minified/TweenMax.min":2,"snapsvg":7,"velocity-animate":8}],19:[function(require,module,exports){
// swiper.js
// =========
// http://idangero.us/swiper/api/
/* global Swiper */
require("./../../../bower_components/Swiper/dist/js/swiper.js");
var debounce = require("./f6/debounce");
var velocity = require("velocity-animate");
var escapeAnim = false;
var timeintro;
var startSwiper2 = false;
var startSwiper1 = false;

var swiperMovePxHover = 90;
var isSwiperAnimating = false;
var customScrollToBloc = true;
var clickTimelineCall = false;

var is_paysage = window.innerHeight < window.innerWidth;
var max_paysage_height = window.innerHeight < 600;
var disableScrollPartMobile = false;


function getDisableScrollPartMobile(){
	if(is_paysage && max_paysage_height){
		disableScrollPartMobile = true;
		jQuery('.partenaire-scroll-1').hide();
	}else{
		disableScrollPartMobile = false;
		jQuery('.partenaire-scroll-1').show();
	}
}
getDisableScrollPartMobile();
window.addEventListener('orientationchange', function(){
	is_paysage = window.innerHeight < window.innerWidth;
	getDisableScrollPartMobile();

});

function moveButtonPartenaire(){

	if(jQuery('.partenaire-scroll-1').parent().hasClass('bntMouseContainer2') && jQuery('.partenaire-mobilse').offset().top > 10){
		jQuery('.partenaire-scroll-1').appendTo(".bntMouseContainer1");
	}else{
		jQuery('.partenaire-scroll-1').appendTo(".bntMouseContainer2");
	}
}

// getActiveHash()
// ---------------
// Return the `data-hash` of the active slide.
function getActiveHash() {
	return document.querySelector(".swiper-container .swiper-slide-active").getAttribute("data-hash");
}

function resizeCustumScroll() {
	jQuery("#page-wrapper").mCustomScrollbar("update");
}
function disableCustumScroll() {
	jQuery("#page-wrapper").mCustomScrollbar("disable");
}

var mySwiper;

function hasClass(element, cls) {
	return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
}

function ajaxLoadBlock(href, hash, cible) {

	jQuery.ajax({
		type: 'POST',
		url: href,
		data: {
			'jo_ajax': 1,
			'jo_node': hash
		},
		success: function (datarespo) {
			var responseData = jQuery.parseJSON(datarespo);

			var dataHtml;
			jQuery.each(responseData, function (i, val) {
				if (val.command === 'insert') {
					dataHtml = val.data;
				}
			});
			if (dataHtml) {				
				cible.html(dataHtml);
			}
			
			jQuery(".swiper-container .swiper-slide[data-hash='" + hash + "']").addClass('pre-loaded');

			jQuery('.site-content.is-front-page').css('height', "auto");
			jQuery("#jo-wrapper-detail, #footer-wrapper").removeClass("hide");
			resizeCustumScroll();

			if (clickTimelineCall) {
				jQuery('.ajax-link').click();
				clickTimelineCall = false;
			}

		}
	});


}

/*jQuery(window).load(function () {
 //resizeCustumScroll();
