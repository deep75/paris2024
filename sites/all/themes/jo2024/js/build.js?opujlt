				scrollInertia: 700
			});*/


	}
	function adjustIframe() {
		jQuery('.swiper-wrapper iframe').each(function () {

			var Docwidth = window.innerWidth;
			Docwidth = Docwidth * 0.5625;
			var DivWidth = "120%";
			var Divdiff = window.innerHeight - Docwidth;

			if (Docwidth < window.innerHeight) {
				Divdiff = Divdiff * 2;
				DivWidth = window.innerWidth + Divdiff + "px";
				jQuery(this).css({
					left: "50%",
					marginLeft: -window.innerWidth / 2 - Divdiff / 2 + "px",
					maxWidth: 'none',
					height: "100%",
					top: 0,
					width: DivWidth
				});


			} else {
				jQuery(this).css({
					left: "0",
					width: "100%",
					marginLeft: 0,
					maxWidth: 'none',
					height: Docwidth,
					top: "-10%"

				});
			}
		});
	}

	jQuery(window).load(function () {
		setTimeout(function () {
			adjustIframe();
		}, 2000);

		jQuery(window).resize(function () {
			adjustIframe();
		}).resize();
	});
});

