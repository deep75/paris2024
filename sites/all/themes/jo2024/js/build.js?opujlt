},{}],16:[function(require,module,exports){
// nav.js
// ======
var isStuck = false;
var stickyElement;

function getDistance() {
	return window.innerHeight;
}

var stickyPoint = getDistance();

function dockNavbar() {
	var offset = window.pageYOffset;

	var distance = getDistance() - offset;

	if ( distance <= 0 && !isStuck) {
		stickyElement.style.position = 'fixed';
		stickyElement.style.top = '0px';
		stickyElement.style.marginTop = "0";
		isStuck = true;
	} else if (isStuck && offset <= stickyPoint){
		stickyElement.style.position = 'static';
		stickyElement.style.marginTop = "0";
		isStuck = false;
	}
}

module.exports = function(element) {
	stickyElement = element;

	window.addEventListener("scroll", dockNavbar, false);
};

