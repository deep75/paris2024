jQuery(document).ready(function () {


	if (jQuery(".ajax-link").length > 0) {
		var ajax_load = true;

		// trigger click key down
		/* document.onkeydown = function () {

			switch (e.keyCode) {
			 case 40:
			 if (jQuery("#jo-wrapper-detail").hasClass("hide")) {
			 jQuery('.ajax-link').click();
			 }
			 break;
			 case 38:
			 var top = jQuery(window).scrollTop();
			 if (top === 0) {
			 jQuery('.element-sticky button.to-top').click();
			 }
			 break;
			 }
		};*/

		jQuery(document).on('click', '.ajax-link', function (e) {

			e.preventDefault();
			clickMouseIcon();

			jQuery('.site-content').removeAttr('style');
			jQuery('.site-content').css({'height': 'auto'});
			var href = jQuery(".hp-content .swiper-slide-active").attr("data-url");
			var hash = getActiveHash();
			var cible = jQuery("#content-" + hash);
			var link = jQuery(this);
			link.addClass("ajax-load");
			var hash_title = jQuery('.hp-content .swiper-slide-active').attr('data-title');
			jQuery('.hp-header .header-text').html(hash_title);

			if (jQuery(".hp-content .swiper-slide-active").hasClass("pre-loaded")) {
				jQuery(".intro-counter").hide();
				if (jQuery(window).innerWidth() < 768) {
					jQuery('body').unbind('touchmove');
				}
				setTimeout(function () {
					jQuery('body').removeClass('hidden-bar');
					jQuery("#jo-wrapper-detail, #footer-wrapper").removeClass("hide");
					jQuery("#page").removeAttr("style");
					jQuery(".to-top").removeClass("hide");
					jQuery(".get-video-wrap").addClass('hide');
					var navintern = setTimeout(function () {
						jQuery(".hp-header").addClass("nav-intern");
						jQuery('.side-menu').addClass('nav-intern-m');
						jQuery('body').addClass('block-n-1');
						clearTimeout(navintern);
					}, 300);
					resizeCustumScroll();
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery("#jo-wrapper-detail"), {scrollInertia: 800});
					customScrollToBloc = false;
					setTimeout(function () {
						killClickMouseIcon();
					}, 500);

				}, 200);

				if (jQuery('.grid-item-description').length > 0) {

					jQuery('.grid-item-description').hover(
						function () {
							jQuery(this).addClass("hovered");
							jQuery(this).removeClass("leave");
						}, function () {
							jQuery(this).removeClass("hovered");
							jQuery(this).addClass("leave");
						}
					);
				}
			}
			else {
				if (ajax_load) {
					ajax_load = false;
					jQuery.ajax({
						type: 'POST',
						url: href,
						data: {
							'jo_ajax': 1,
							'jo_node': hash
						},
						success: function (datarespo) {
							jQuery('body').removeClass('hidden-bar');
							var responseData = jQuery.parseJSON(datarespo);

							var dataHtml;
							jQuery.each(responseData, function (i, val) {
								if (val.command === 'insert') {
									dataHtml = val.data;
								}
							});
							if (dataHtml) {
								cible.html(dataHtml);
								jQuery("#jo-wrapper-detail, #footer-wrapper").removeClass("hide");
							}
							link.removeClass("ajax-load");
							jQuery(".hp-content .swiper-slide-active").addClass("pre-loaded");
							ajax_load = true;

							killClickMouseIcon();
						}
					}).complete(function () {
						if (jQuery(window).innerWidth() < 768) {
							jQuery('body').unbind('touchmove');
						}
						jQuery("#page").removeAttr("style");
						jQuery(".to-top").removeClass("hide");
						jQuery(".get-video-wrap").addClass('hide');
						var navintern = setTimeout(function () {
							jQuery(".hp-header").addClass("nav-intern");
							jQuery('.side-menu').addClass('nav-intern-m');
							jQuery('body').addClass('block-n-1');
							clearTimeout(navintern);
						}, 300);
						resizeCustumScroll();

						jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery("#jo-wrapper-detail"), {scrollInertia: 800});
						customScrollToBloc = false;
						setTimeout(function () {
							killClickMouseIcon();
						}, 500);
						ajax_load = true;
						jQuery(".intro-counter").hide();
					});
				}
			} // End else
		});

	}

	jQuery(document).on('click', '.to-top', function (e) {
		e.preventDefault();
		if (jQuery('#jo-wrapper-detail').length) {
			var windowtop = jQuery(window).scrollTop() - 2;
			if (windowtop > jQuery('#jo-wrapper-detail').offset().top) {
				jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery("#jo-wrapper-detail"), {scrollInertia: 600});
				customScrollToBloc = false;
			} else {
				if (jQuery(window).innerWidth() < 768) {
					jQuery("body").on("touchmove", false);
					var addheight2 = jQuery(window).innerHeight() + 10;
					var vartimeoutimg = setTimeout(function () {
						jQuery('.image-wrapper').each(function () {
							jQuery(this).find('img').addClass('teste').css({
								'top': 'auto',
								'bottom': '-5px',
								'height': addheight2 + 'px'
							});
						});
						clearTimeout(vartimeoutimg);
					}, 200);
				}

				jQuery("#page-wrapper").mCustomScrollbar("scrollTo", 0, {scrollInertia: 700});
				customScrollToBloc = false;

				jQuery(".hp-header").removeClass("nav-intern");
				jQuery('.side-menu').removeClass('nav-intern-m');
				jQuery('body').removeClass('block-n-1');
				jQuery(".to-top").addClass("hide");
				jQuery(".get-video-wrap").removeClass('hide');
				var vartimeout = setTimeout(function () {
					jQuery('.show').each(function () {
						jQuery(this).removeClass('show');
					});
					jQuery('.leave').each(function () {
						jQuery(this).removeClass('leave');
					});
					jQuery('.leave').each(function () {
						jQuery(this).removeClass('leave');
					});
					clearTimeout(vartimeout);
				}, 750);

			}
		} else if(!jQuery('#page-partenaires').length) {
			jQuery("#page-wrapper").mCustomScrollbar("scrollTo", 0, {scrollInertia: 700});
			customScrollToBloc = false;

		} else if(jQuery('#page-partenaires').length){
			var partMobilise = jQuery('.partenaire-mobilse');
			var partEngage = jQuery('.partenaire-engage');

			if(disableScrollPartMobile){
				jQuery("#page-wrapper").mCustomScrollbar("scrollTo", 0, {scrollInertia: 700, callbacks:false});
			}else{
				if (jQuery('.partenaire-mobilse').offset().top >= 0) {
					//top
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", 0, {scrollInertia: 700, callbacks:false});
					setTimeout(function(){
						moveButtonPartenaire();
					}, 700);

					jQuery('.to-top').addClass('hide');
				} else if (jQuery('.partenaire-mobilse').offset().top >= -partMobilise.outerHeight()) {
					// middle
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", partMobilise, {scrollInertia: 700, callbacks:false});
				}else{
					// bottom
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", partEngage, {scrollInertia: 700, callbacks:false});
				}
			}

		}

	});

jQuery(document).resize(function () {
	if (jQuery(document).height() < jQuery('.side-heart #coeur-block').innerHeight()) {
		jQuery('.side-heart').mCustomScrollbar({
			theme: "dark",
			scrollbarPosition: "outside",
			axis: "y",
			keyboard: {scrollAmount: 40},
			mouseWheel: {deltaFactor: 40},
			scrollInertia: 700
		});
	} else {
		jQuery('.side-heart').mCustomScrollbar("destroy");
	}
}).resize();

function adjustPartIframe() {
	jQuery('.header-partenaire iframe').each(function () {

		var Docwidth = window.innerWidth;
		Docwidth = Docwidth * 0.5625;
		var DivWidth = "120%";
		var Divdiff = window.innerHeight - Docwidth;

		if (Docwidth < window.innerHeight) {
			Divdiff = Divdiff * 2;
			DivWidth = window.innerWidth + Divdiff + "px";
			jQuery(this).css({
				left: "50%",
				marginLeft: -window.innerWidth / 2 - Divdiff / 2 + "px",
				maxWidth: 'none',
				height: "100%",
				top: 0,
				width: DivWidth
			});


		} else {
			jQuery(this).css({
				left: "0",
				width: "100%",
				marginLeft: 0,
				maxWidth: 'none',
				height: Docwidth,
				top: "-10%"

			});
		}
	});
}

/* page partenaire */
function adjustImagePartenaireSize() {
	adjustPartIframe();
		// list of cover images
		var imgs = document.querySelectorAll(".header-partenaire .cover");

		// window aspect ratio
		var i = 0, len;

		var windowWidthA = jQuery(window).innerWidth();
		var windowHeightA = jQuery(window).innerHeight();
		var R = windowWidthA / windowHeightA;
		var res = 0;
		for (i = 0, len = imgs.length; i < len; i++) {
			var is_img = hasClass(imgs[i], "is-image");
			var r;
			if (is_img) {
				r = imgs[i].naturalWidth / imgs[i].naturalHeight;
			} else {
				r = imgs[i].videoWidth / imgs[i].videoHeight;
			}
			var imageWidthA;
			var imageHeightA;


			if (is_img) {
				imageWidthA = imgs[i].naturalWidth;
				imageHeightA = imgs[i].naturalHeight;
			} else {
				imageWidthA = imgs[i].videoWidth;
				imageHeightA = imgs[i].videoHeight;
			}



			var caseImage;

			if (R === r) {
				caseImage = 3;
			} else if (R > r) {
				caseImage = 2;
			} else if (R < r) {
				caseImage = 1;
			}

			switch (caseImage) {
				case 1:
					imgs[i].style.height = "100vh";
					if (windowWidthA < 768) {
						var addheight = windowHeightA + 10;
						imgs[i].style.height = addheight + "px";
					}

					res = - imgs[i].offsetWidth/2 + windowWidthA/2;

					imgs[i].style.marginLeft = res + "px";
					imgs[i].style.marginTop = 0;
					imgs[i].style.top = 0;
					imgs[i].style.bottom = 0;
					if (windowWidthA < 768) {
						imgs[i].style.top = 'auto';
						imgs[i].style.bottom = '-5px';
					}
					imgs[i].style.width = "auto";
					break;
				case 2:

					imgs[i].style.height = "auto";
					imgs[i].style.left = 0;
					imgs[i].style.marginLeft = 0;
					imgs[i].style.width = "100vw";
					break;
				default:
					imgs[i].style.height = "auto";
					imgs[i].style.left = 0;
					imgs[i].style.marginLeft = 0;
					imgs[i].style.marginTop = 0;
					imgs[i].style.top = 0;
					imgs[i].style.width = "auto";
			}
		}
	}
	var swiper2;
	var slidesPerColumn = 2;

function partenairesSwipeResize(slidesPerColumn1){

		swiper2 = new Swiper('.swiper-container-part', {
			pagination: '.swiper-pagination',
			slidesPerView: 3,
			slidesPerColumn: slidesPerColumn1,
			paginationClickable: true,
			spaceBetween: 20,
			slidesPerGroup: 3,
			breakpoints: {
				// when window width is <= 320px
				767: {
					slidesPerView: 1,
					slidesPerColumn: 1,
					slidesPerGroup: 1,
					spaceBetween: 20
				},
				// when window width is <= 480px
				1023: {
					slidesPerView: 2,
					slidesPerGroup: 2,
					slidesPerColumn: slidesPerColumn1,
					spaceBetween: 20
				}
			},
			onSliderMove: function(){
				disableCustumScroll();
				startSwiper2 = true;
			},
			onTouchEnd: function(){
				resizeCustumScroll();
				jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery(".partenaire-mobilse"), {scrollInertia: 0});
				setTimeout(function(){
					startSwiper2 = false;
				}, 300);
			}
		});
	}
function adjustHeightPatnersMobilise(){

		var mobHeight = 0;
		var mobHtml = jQuery('#page-partenaires .partenaire-mobilse .descr-wrap');
		mobHtml.each(function(){
			if(jQuery(this).innerHeight() > mobHeight){
				mobHeight = jQuery(this).innerHeight();
			}
		});
	if(jQuery(window).width() >= 768){
		mobHeight += 10;
	}else{
		mobHeight -= 5;
	}
	mobHtml.css('height', mobHeight+"px");
}
if(jQuery('#page-partenaires').length){



		jQuery('#page-partenaires .partenaire-mobilse .swiper-slide img,#page-partenaires .partenaire-mobilse .swiper-slide h3,#page-partenaires .partenaire-mobilse .swiper-slide .descr').click(function(e){
			e.stopPropagation();
			jQuery(this).parent().find(".to-popin").trigger('click');
			jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery(".partenaire-mobilse"), {scrollInertia: 0});
		});

		require('jquery-colorbox');

		if(jQuery("#page-partenaires .video-wrapper video").length){
			jQuery("#page-partenaires .video-wrapper video").load();
		}
		if(jQuery('#page-partenaires .to-popin').length){
			jQuery('#page-partenaires .to-popin').colorbox({inline: true, maxHeight: "100%", maxWidth: "70%", width: "1280px", height: "auto",onComplete: function() {
					jQuery('#cboxClose').empty();
				 },
				onClosed: function() {
					jQuery("#page-wrapper").mCustomScrollbar("scrollTo", jQuery(".partenaire-mobilse"), {scrollInertia: 0});
				}});
		}
	adjustHeightPatnersMobilise();

	partenairesSwipeResize(slidesPerColumn);


		setTimeout(function(){
			jQuery('.partenaire-mobilse .partenaire-mobilse-table .partenaire-mobilse-table-cell').removeClass('active');
			var ww = jQuery("#page-partenaires .partenaire-mobilse").height();
			var wwc = jQuery("#page-partenaires .partenaire-mobilse").children().outerHeight(true);
			var swiper1;
			if(wwc > ww && jQuery(window).width() > 767) {
				slidesPerColumn = 1;
				swiper1 = jQuery('.swiper-container-part')[0].swiper;
				if (typeof swiper1 !== 'undefined'){
					swiper1.destroy(true, true);
				}
				partenairesSwipeResize(slidesPerColumn);

			}else if(jQuery(window).width() > 767){
				swiper1 = jQuery('.swiper-container-part')[0].swiper;
				if (typeof swiper1 !== 'undefined'){
					swiper1.destroy(true, true);
				}
				slidesPerColumn = 2;
				partenairesSwipeResize(slidesPerColumn);
			}else if(jQuery(window).width() <= 767){
				swiper1 = jQuery('.swiper-container-part')[0].swiper;
				if (typeof swiper1 !== 'undefined'){
					swiper1.destroy(true, true);
				}
				partenairesSwipeResize(slidesPerColumn);
			}
		}, 500);


